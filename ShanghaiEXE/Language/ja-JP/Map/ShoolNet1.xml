  <Dialogue Key="Map.ShoolNet1MarisaStage1Dialogue2FaceMarisa" Value="シャンハイ,お前は道が見えるか？" Face="Marisa" Mono="False" />
  <Dialogue Key="Map.ShoolNet1SakuyaStage1Dialogue1FaceSakuya" Value="くっ････,なかなかウィルスが,てごわいですわね････！" Face="Sakuya" Mono="False" />
  <Dialogue Key="Map.ShoolNet1SpannerManStage1Dialogue1FaceSpannerMan" Value="ダメだ　通れねえ！" Face="SpannerMan" Mono="False" />
