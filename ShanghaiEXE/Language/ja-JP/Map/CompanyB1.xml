  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue12FaceReisen" Value="神綺アリス　行くぞ！" Face="Reisen" Mono="False" />
  <!-- ええ！ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue13FaceAlice" Value="ええ！" Face="Alice" Mono="False" />
