  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue1FaceReisen" Value="これからどうする？" Face="Reisen" Mono="False" />
  <!-- アリスに危害を加えやがってアッタマに来た！ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue2FaceShanghai" Value="アリスに危害を加えやがって,アッタマに来た！" Face="Shanghai" Mono="False" />
  <!-- こうなりゃ社長のやろうに直談判してやるわ！ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue3FaceShanghai" Value="こうなりゃ社長のやろうに,直談判してやるわ！" Face="Shanghai" Mono="False" />
  <!-- フム　珍しく意見があったな -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue4FaceReisen" Value="フム　珍しく意見があったな" Face="Reisen" Mono="False" />
  <!-- 鈴仙　手伝ってくれるの？ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue5FaceAlice" Value="鈴仙　手伝ってくれるの？" Face="Alice" Mono="False" />
  <!-- 一連の行動からこの企業は完全にクロと断定した -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue6FaceReisen" Value="一連の行動から,この企業は完全にクロと,断定した" Face="Reisen" Mono="False" />
  <!-- ならば　遠慮する必要はない -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue7FaceReisen" Value="ならば　遠慮する必要はない" Face="Reisen" Mono="False" />
