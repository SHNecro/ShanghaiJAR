  <!-- ボーダーコンツェルン　地下牢 -->
  <Text Key="Map.CompanyB1Name" Value="ボーダーコンツェルン　地下牢" />
  <!-- ううん････ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage1Dialogue1FaceAlice" Value="ううん････" Face="Alice" Mono="False" />
  <!-- あれ？確かプラグインしたあと急にビリッとして････ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage1Dialogue2FaceAlice" Value="あれ？,確かプラグインしたあと,急にビリッとして････" Face="Alice" Mono="False" />
  <!-- 目が覚めたか････ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage1Dialogue3FaceNone" Value="目が覚めたか････" Face="None" Mono="False" />
