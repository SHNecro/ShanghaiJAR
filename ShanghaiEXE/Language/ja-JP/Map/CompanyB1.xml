  <Dialogue Key="Map.CompanyB1Chapter7EventsStage7Dialogue1FaceShanghai" Value="アリス　そこにいるのね！" Face="Shanghai" Mono="False" />
  <!-- シャンハイ！ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage7Dialogue2FaceAlice" Value="シャンハイ！" Face="Alice" Mono="False" />
  <!-- ････よし！社内無線ネットワークからアリスの携帯へ接続できたわ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage7Dialogue3FaceShanghai" Value="････よし！,社内無線ネットワークから,アリスの携帯へ接続できたわ" Face="Shanghai" Mono="False" />
  <!-- これでオペレートできるわねアリス　大丈夫？ -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage7Dialogue4FaceShanghai" Value="これでオペレートできるわね,アリス　大丈夫？" Face="Shanghai" Mono="False" />
