  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue10FaceShanghai" Value="あたしはこのまま,サーバーの中から,あんたたちを支援するわ" Face="Shanghai" Mono="False" />
  <!-- プラグアウトはできないからシャンハイ　気をつけてね -->
  <Dialogue Key="Map.CompanyB1Chapter7EventsStage9Dialogue11FaceAlice" Value="プラグアウトはできないから,シャンハイ　気をつけてね" Face="Alice" Mono="False" />
