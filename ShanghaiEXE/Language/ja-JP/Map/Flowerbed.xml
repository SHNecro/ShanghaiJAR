<?xml version="1.0" encoding="utf-8"?>
<data>
  <!-- 花だんの電脳 -->
  <Text Key="Map.FlowerbedName" Value="花だんの電脳" />
  <!-- お待ちしておりましたシャンハイ様 -->
  <Dialogue Key="Map.FlowerbedRequest14YumekoStage1Dialogue1FaceNaviRedSprite" Value="お待ちしておりました,シャンハイ様" Face="NaviRedSprite" Mono="False" />
  <!-- あなたって私の家のスプライトちゃん？ -->
  <Dialogue Key="Map.FlowerbedRequest14YumekoStage1Dialogue2FaceAlice" Value="あなたって私の家の,スプライトちゃん？" Face="Alice" Mono="False" />
  <!-- 指定した場所でだいたい予想はついてたけどね -->
  <Dialogue Key="Map.FlowerbedRequest14YumekoStage1Dialogue3FaceShanghai" Value="指定した場所でだいたい,予想はついてたけどね" Face="Shanghai" Mono="False" />
