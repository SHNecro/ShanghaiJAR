  <Dialogue Key="Map.RemihouseSakuyaBattleStage11Dialogue2FaceRemilia" Value="気が向いた時に,私はいつでも受けて立つわよ" Face="Remilia" Mono="False" />
  <Dialogue Key="Map.RemihouseBookShelf1Stage1Dialogue1FaceNone" Value="レミリアの本棚だ" Face="None" Mono="False" />
