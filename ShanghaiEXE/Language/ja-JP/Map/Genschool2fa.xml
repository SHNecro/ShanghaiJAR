<data>
  <Text Key="Map.Genschool2faName" Value="幻想中学校　2F" />
  <Dialogue Key="Map.Genschool2faToWarehouseStage1Dialogue1FaceNone" Value="倉庫へ通じる扉だが,カギがかかって入れない" Face="None" Mono="False" />
  <Dialogue Key="Map.Genschool2faToMultipurposeRoomStage1Dialogue1FaceNone" Value="多目的室への扉だ" Face="None" Mono="False" />
  <Dialogue Key="Map.Genschool2faToMultipurposeRoomStage1Dialogue2FaceNone" Value="入学してからこの部屋が,使われているところを,見たことがない" Face="None" Mono="False" />
  <Dialogue Key="Map.Genschool2faTo1FStage1Dialogue1FaceShanghai" Value="ちょっと　授業を受けずに,どこに行くつもりなの！？" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.Genschool2faTo1FStage3Dialogue1FaceShanghai" Value="帰る前にまだやることが,あるでしょーが！" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.Genschool2faTo1FStage5Dialogue1FaceAlice" Value="シャンハイを置いてはいけない！" Face="Alice" Mono="False" />
  <Dialogue Key="Map.Genschool2faChapter1Woman1Stage1Dialogue1FaceFemaleStudent" Value="こまったなー･･･" Face="FemaleStudent" Mono="False" />
  <Dialogue Key="Map.Genschool2faChapter1Woman1Stage1Dialogue2FaceFemaleStudent" Value="倉庫に用があるのに,先生まだ来ないなあ" Face="FemaleStudent" Mono="False" />
