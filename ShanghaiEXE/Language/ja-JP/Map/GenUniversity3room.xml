  <Dialogue Key="Map.GenUniversity3roomRemiliaStage3Dialogue13FaceShanghai" Value="黙って聞く！" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity3roomRemiliaStage3Dialogue14FaceShanghai" Value="レミリア　あなたはこのまま,弱虫でいいの？" Face="Shanghai" Mono="False" />
