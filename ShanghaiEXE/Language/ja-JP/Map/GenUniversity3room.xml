  <Dialogue Key="Map.GenUniversity3roomRemiliaStage3Dialogue22FaceShanghai" Value="能あるタカは爪を隠す････" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity3roomRemiliaStage3Dialogue23FaceShanghai" Value="あなたは能あり能なし,どっちかしら？" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity3roomRemiliaStage3Dialogue24FaceRemilia" Value="グスン････" Face="Remilia" Mono="False" />
