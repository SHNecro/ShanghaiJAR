  <Dialogue Key="Map.GenNet1Chapter2GirlNavi1Stage1Dialogue1FaceNaviPinkFemale" Value="サブチップを売ってる店って,いっぱいあるわよね" Face="NaviPinkFemale" Mono="False" />
