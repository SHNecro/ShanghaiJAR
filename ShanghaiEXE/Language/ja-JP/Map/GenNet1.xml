  <Dialogue Key="Map.GenNet1Chapter2MaleNavi1Stage1Dialogue2FaceNaviGreenRobot" Value="カウンターでとどめを刺すと,その敵からバグのかけらとかが,手に入るんだよ" Face="NaviGreenRobot" Mono="False" />
  <!-- 何のかけらが手に入るかは敵の属性で変わるんだって -->
  <Dialogue Key="Map.GenNet1Chapter2MaleNavi1Stage1Dialogue3FaceNaviGreenRobot" Value="何のかけらが手に入るかは,敵の属性で変わるんだって" Face="NaviGreenRobot" Mono="False" />
