  <!-- インターネット　ゲンソウエリア1 -->
  <Text Key="Map.GenNet1Name" Value="インターネット　ゲンソウエリア1" />
  <!-- この四角い物体はセキュリティーキューブって言うんだよ -->
  <Dialogue Key="Map.GenNet1Chapter1ManNavi1Stage1Dialogue1FaceNaviGreenRobot" Value="この四角い物体は,セキュリティーキューブ,って言うんだよ" Face="NaviGreenRobot" Mono="False" />
