  <!-- 「セイレーンフォルダ」を予備フォルダにしますか？ -->
  <Dialogue Key="Map.SeirenShip2fSPSeirenFolderStage1Dialogue4QuestionFaceWhiteHairMaleButler" Value="「セイレーンフォルダ」を,予備フォルダにしますか？" Face="WhiteHairMaleButler" Mono="False" />
  <!-- よろしいよろしくない -->
  <Text Key="Map.SeirenShip2fSPSeirenFolderStage1Dialogue4Options" Value="よろしい,よろしくない" />
  <!-- ・・・・・・・・#s@docking#ピコーン！ -->
  <Dialogue Key="Map.SeirenShip2fSPSeirenFolderStage3Dialogue1FaceWhiteHairMaleButler" Value="・・・・・・・・#s@docking#ピコーン！" Face="WhiteHairMaleButler" Mono="False" />
