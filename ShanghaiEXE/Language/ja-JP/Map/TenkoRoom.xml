<?xml version="1.0" encoding="utf-8"?>
<data>
  <!-- 天子の部屋 -->
  <Text Key="Map.TenkoRoomName" Value="天子の部屋" />
  <!-- アリス　いらっしゃいなのよ！ -->
  <Dialogue Key="Map.TenkoRoomTenshiStage1Dialogue1FaceTenshi" Value="アリス　いらっしゃいなのよ！" Face="Tenshi" Mono="False" />
  <!-- 友達がお家に来るのってなんだかドキドキするのよ！ -->
  <Dialogue Key="Map.TenkoRoomTenshiStage1Dialogue2FaceTenshi" Value="友達がお家に来るのって,なんだかドキドキするのよ！" Face="Tenshi" Mono="False" />
  <!-- おかえりなのよ！アリス！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage1Dialogue1FaceTenshi" Value="おかえりなのよ！,アリス！" Face="Tenshi" Mono="False" />
  <!-- 退院おめでとう　天子！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage1Dialogue2FaceAlice" Value="退院おめでとう　天子！" Face="Alice" Mono="False" />
  <!-- はい　これおみやげ！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage1Dialogue3FaceAlice" Value="はい　これおみやげ！" Face="Alice" Mono="False" />
  <!-- わあい！キレイなブレスレットなのよ！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage1Dialogue4FaceTenshi" Value="わあい！,キレイなブレスレットなのよ！" Face="Tenshi" Mono="False" />
  <!-- 退院祝いをかねてね！ちょっと奮発しちゃったの！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage1Dialogue5FaceAlice" Value="退院祝いをかねてね！,ちょっと奮発しちゃったの！" Face="Alice" Mono="False" />
  <!-- ありがとうなのよ！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage1Dialogue6FaceTenshi" Value="ありがとうなのよ！" Face="Tenshi" Mono="False" />
  <!-- （お金はあたしが　出したんだけどなあ････　まあ　いっか） -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage1Dialogue7FaceShanghai" Value="（お金はあたしが,　出したんだけどなあ････,　まあ　いっか）" Face="Shanghai" Mono="False" />
  <!-- これでおみやげは全員に配り終えたわね -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage3Dialogue1FaceShanghai" Value="これでおみやげは,全員に配り終えたわね" Face="Shanghai" Mono="False" />
  <!-- あとは帰ってママにただいま言いましょ！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage3Dialogue2FaceShanghai" Value="あとは帰って,ママにただいま言いましょ！" Face="Shanghai" Mono="False" />
  <!-- おみやげ　ありがとうなのよ！大切にするのよ！ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage5Dialogue1FaceTenshi" Value="おみやげ　ありがとうなのよ！,大切にするのよ！" Face="Tenshi" Mono="False" />
  <!-- そうだ！退院と帰国のお祝いにネットバトルしない？ -->
  <Dialogue Key="Map.TenkoRoomChapter6Stage5Dialogue2FaceTenshi" Value="そうだ！,退院と帰国のお祝いに,ネットバトルしない？" Face="Tenshi" Mono="False" />
  <!-- アリスさんおかえりなのです！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage1Dialogue1FaceRika" Value="アリスさんおかえりなのです！" Face="Rika" Mono="False" />
  <!-- 里香　ただいまー！おみやげもあるよ！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage1Dialogue2FaceAlice" Value="里香　ただいまー！,おみやげもあるよ！" Face="Alice" Mono="False" />
  <!-- はわわっ！これはインゲレス軍新型戦車のプラモデルなのです！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage1Dialogue3FaceRika" Value="はわわっ！,これはインゲレス軍新型戦車の,プラモデルなのです！" Face="Rika" Mono="False" />
  <!-- すっごくうれしいのです！！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage1Dialogue4FaceRika" Value="すっごくうれしいのです！！" Face="Rika" Mono="False" />
  <!-- どういたしまして！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage1Dialogue5FaceAlice" Value="どういたしまして！" Face="Alice" Mono="False" />
  <!-- これでおみやげは全員に配り終えたわね -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage3Dialogue1FaceShanghai" Value="これでおみやげは,全員に配り終えたわね" Face="Shanghai" Mono="False" />
  <!-- あとは帰ってママにただいま言いましょ！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage3Dialogue2FaceShanghai" Value="あとは帰って,ママにただいま言いましょ！" Face="Shanghai" Mono="False" />
  <!-- おみやげすっごく嬉しいのです！感謝感激なのなのです！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage5Dialogue1FaceRika" Value="おみやげすっごく嬉しいのです！,感謝感激なのなのです！" Face="Rika" Mono="False" />
  <!-- シャンハイがいない間タンクマン寂しがってたのでネットバトルするのです！ -->
  <Dialogue Key="Map.TenkoRoomChapter6RikaStage5Dialogue2FaceRika" Value="シャンハイがいない間,タンクマン寂しがってたので,ネットバトルするのです！" Face="Rika" Mono="False" />
  <!-- 天子にネットバトルを申し込む？ -->
  <Dialogue Key="Map.TenkoRoomIkutororuStage1Dialogue1QuestionFaceShanghai" Value="天子にネットバトルを申し込む？" Face="Shanghai" Mono="False" />
  <!-- はいいいえ -->
  <Text Key="Map.TenkoRoomIkutororuStage1Dialogue1Options" Value="はい,いいえ" />
  <!-- イク！頑張ってね！ -->
  <Dialogue Key="Map.TenkoRoomIkutororuStage1Dialogue2FaceTenshi" Value="イク！,頑張ってね！" Face="Tenshi" Mono="False" />
  <!-- わーい！私が勝ったのよ！ -->
  <Dialogue Key="Map.TenkoRoomIkutororuStage3Dialogue1FaceTenshi" Value="わーい！,私が勝ったのよ！" Face="Tenshi" Mono="False" />
  <!-- アリスってすごく強いね！ -->
  <Dialogue Key="Map.TenkoRoomIkutororuStage5Dialogue1FaceTenshi" Value="アリスってすごく強いね！" Face="Tenshi" Mono="False" />
  <!-- 天子様のカスタマイズを受けパワーアップしましたわ！ -->
  <Dialogue Key="Map.TenkoRoomIkutororuStage6Dialogue1FaceIku" Value="天子様のカスタマイズを受け,パワーアップしましたわ！" Face="Iku" Mono="False" />
