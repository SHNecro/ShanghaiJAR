  <Text Key="Map.ClockTower4GimmickStage1Dialogue1Options" Value="예,아니오" />
  <!-- 短針をどこに動かす？ -->
  <Dialogue Key="Map.ClockTower4GimmickStage3Dialogue1QuestionFaceNone" Value="시침을 어디로 움직일까?" Face="None" Mono="True" />
