  <!-- その前！ -->
  <Dialogue Key="Map.CenterccityNorthChapter8WriggleStage11Dialogue7FaceShanghai" Value="아냐，그보다 더 앞에,말한 거 있잖아!" Face="Shanghai" Mono="False" />
  <!-- 新型のステルス機に乗ってきたですって！？ -->
