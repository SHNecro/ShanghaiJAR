  <!-- イン！ -->
  <Dialogue Key="Map.NetAgentSenter1fChapter4ReimuStage1Dialogue17FaceShanghai" Value="인!" Face="Shanghai" Mono="False" />
  <!-- これで出るようになったはずよ -->
  <Dialogue Key="Map.NetAgentSenter1fChapter4ReimuStage3Dialogue1FaceAlice" Value="이러면 이제 문제 없이 나오겠지." Face="Alice" Mono="False" />
  <!-- えーっと　このジュースね -->
  <Dialogue Key="Map.NetAgentSenter1fChapter4ReimuStage3Dialogue2FaceAlice" Value="어디 보자，이 주스지?" Face="Alice" Mono="False" />
