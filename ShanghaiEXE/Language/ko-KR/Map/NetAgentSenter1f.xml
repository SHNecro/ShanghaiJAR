  <!-- 好きなジュースなんだけど位置が変わっちゃったの -->
  <Dialogue Key="Map.NetAgentSenter1fChapter4ReimuStage1Dialogue4FaceReimu" Value="좋아하는 주스였는데,위치가 바뀌어 버렸어." Face="Reimu" Mono="False" />
  <!-- 確かにちょっと高いボタンね -->
