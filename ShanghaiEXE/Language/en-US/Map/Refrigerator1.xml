  <Dialogue Key="Map.Refrigerator1IceEventStage1Dialogue1FaceShanghai" Value="....!!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage1Dialogue2FaceAlice" Value="Shanghai?,What’s wrong?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage1Dialogue3FaceShanghai" Value="Something’s....coming!?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage3Dialogue1FaceAlice" Value="Shanghai!,Answer me!,Shanghai!!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage3Dialogue2FaceShanghai" Value="....Phew!,The ice finally melted!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage3Dialogue3FaceShanghai" Value="You saved me，Alice?,Thanks!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage3Dialogue4FaceAlice" Value="Gladly!,But we don’t have much time!,We have to stop the fridge!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage3Dialogue5FaceShanghai" Value="Of course!,I was thinking the same thing!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.Refrigerator1IceEventStage3Dialogue6FaceShanghai" Value="I’ll beat whatever’s causing,this within an inch of deletion!!" Face="Shanghai" Mono="False" />
