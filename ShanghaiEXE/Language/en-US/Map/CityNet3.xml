<?xml version="1.0" encoding="utf-8"?>
<data>
  <!-- インターネット　シティエリア３ -->
  <Text Key="Map.CityNet3Name" Value="City Area 3" />
  <!-- 倒したナビの残骸データがどこかへと飛び去っていった････ -->
  <Dialogue Key="Map.CityNet3ChenV2Stage1Dialogue1FaceNone" Value="The defeated Navi’s remains,flew away...." Face="None" Mono="False" />
  <!-- ←：シティエリア１↓：エイエンエリア１ -->
  <Dialogue Key="Map.CityNet3GuidePlateStage1Dialogue1FaceNone" Value="←: City Area 1,↓: Eien Area 1" Face="None" Mono="False" />
  <!-- ネットエージェントによって一時的に封鎖されているようだ -->
  <Dialogue Key="Map.CityNet3GateStage1Dialogue1FaceNone" Value="The path seems to be,temporarily blocked by,the NetAgents." Face="None" Mono="False" />
  <!-- このエリアがすっごく複雑なのはここからいろんなオフィスにつながってるからなのよ -->
  <Dialogue Key="Map.CityNet3Chapter2GirlNavi1Stage1Dialogue1FaceNaviPinkFemale" Value="This area is complex because,it’s connected to various,offices." Face="NaviPinkFemale" Mono="False" />
  <!-- あれ？スクエアこっちじゃないの？ -->
  <Dialogue Key="Map.CityNet3Chapter2YoungGirlNavi1Stage1Dialogue1FaceNaviChild" Value="Wha?,The Square isn’t here?" Face="NaviChild" Mono="False" />
  <!-- うわーん迷っちゃったー！ -->
  <Dialogue Key="Map.CityNet3Chapter2YoungGirlNavi1Stage1Dialogue2FaceNaviChild" Value="Waaaaah! I got loooost!" Face="NaviChild" Mono="False" />
  <!-- どこかに毒ヘビのウィルスが出るらしいけど　そいつの落とすチップは特殊なんだって -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi1Stage1Dialogue1FaceNaviOrangeMale" Value="There’s a venomous snake,virus somewhere that drops,a special chip." Face="NaviOrangeMale" Mono="False" />
  <!-- なんでも　Ａボタンを押しているあいだだけ攻撃するんだって途中でやめられるってことだ -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi1Stage1Dialogue2FaceNaviOrangeMale" Value="You only attack while holding,down A，so you can stop,attacking at any time!" Face="NaviOrangeMale" Mono="False" />
  <!-- 毒属性は炎で消毒したり雷で電気分解するからそれらに弱いらしいよ -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi2Stage1Dialogue1FaceNaviOrangeMale" Value="Poison is sterilized by heat and,electrolyzed by electricity!" Face="NaviOrangeMale" Mono="False" />
  <!-- 逆に　水を腐らせたり草を枯らせられるからそれらには強いんだ -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi2Stage1Dialogue2FaceNaviOrangeMale" Value="However，poison pollutes,water and kills grass!" Face="NaviOrangeMale" Mono="False" />
  <!-- ＲＯＭへの警戒のためってネットエージェントが道を塞いでます -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi3Stage1Dialogue1FaceNaviGreenRobot" Value="NetAgents are blocking the,roads to try to herd a ROM,Navi." Face="NaviGreenRobot" Mono="False" />
  <!-- エイエンエリアへの唯一の道なのですが困りましたね -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi3Stage1Dialogue2FaceNaviGreenRobot" Value="But they blocked the only road,to Eien Area，so I’m stuck here." Face="NaviGreenRobot" Mono="False" />
  <!-- 奥に見えるワープってボーダーコンツェルンの本社につながってるらしいね -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi4Stage1Dialogue1FaceNaviGreenRobot" Value="Seems like the warp in the,middle goes to Border,Concern’s Headquarters." Face="NaviGreenRobot" Mono="False" />
  <!-- でも　あそこに行く方法は誰もしらないんだって -->
  <Dialogue Key="Map.CityNet3Chapter2MaleNavi4Stage1Dialogue2FaceNaviGreenRobot" Value="But nobody knows how,to get over there." Face="NaviGreenRobot" Mono="False" />
  <!-- 何じゃあ？千里眼じいさんを探しとるじゃと？ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue1FaceNaviOrangeRobot" Value="What is it?,Are you looking for the Sage?" Face="NaviOrangeRobot" Mono="False" />
  <!-- いかにもわしが検索ソフト「千里眼」を持っとるじじいナビじゃが････ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue2FaceNaviOrangeRobot" Value="Indeed，I’m the Sage.,I’m the Navi that holds the,”ESP” search engine...." Face="NaviOrangeRobot" Mono="False" />
  <!-- なに？　探しものとな？ふむ････協力してやらんでもないが････ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue3FaceNaviOrangeRobot" Value="Are you looking for something?,Hm....,Let’s help each other out...." Face="NaviOrangeRobot" Mono="False" />
  <!-- そうじゃな「リペア２０　＊」と引き換えに探してやっても良いぞ？ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue4FaceNaviOrangeRobot" Value="How about I perform the,search in exchange for a,”Recov20 ＊”?" Face="NaviOrangeRobot" Mono="False" />
  <!-- 「リペア２０　＊」は持っておるかの？ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue5QuestionFaceNaviOrangeRobot" Value="Do you have a,”Recov20 ＊”?" Face="NaviOrangeRobot" Mono="False" />
  <!-- はいいいえ -->
  <Text Key="Map.CityNet3Chapter3SageStage1Dialogue5Options" Value="Yes,No" />
  <!-- 持っておらんではないか！ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue6FaceNaviOrangeRobot" Value="You don’t have one!" Face="NaviOrangeRobot" Mono="False" />
  <!-- もしかしてチップフォルダに入れてるんじゃなかろうな？ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue7FaceNaviOrangeRobot" Value="Maybe it’s in your folder?" Face="NaviOrangeRobot" Mono="False" />
  <!-- もしもそうならばフォルダから外してくれんと受け取れんよ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue8FaceNaviOrangeRobot" Value="If that’s the case，you need,to take it out of the folder." Face="NaviOrangeRobot" Mono="False" />
  <!-- これじゃこれじゃ強い回復チップは老いぼれた身体には毒じゃ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue9FaceNaviOrangeRobot" Value="There we are.,Potent healing chips are toxic,to an ancient Navi like me." Face="NaviOrangeRobot" Mono="False" />
  <!-- ふむ　約束じゃそなたの探しているものを検索してやろう -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue10FaceNaviOrangeRobot" Value="Hm.... As promised，,I’ll search for whatever,it is you’re looking for." Face="NaviOrangeRobot" Mono="False" />
  <!-- むむむむむ・・・・ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue11FaceNaviOrangeRobot" Value="Hmmmmmm...." Face="NaviOrangeRobot" Mono="False" />
  <!-- カーーーーッ！ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue12FaceNaviOrangeRobot" Value="Ding!" Face="NaviOrangeRobot" Mono="False" />
  <!-- 見えたぞ！！ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue13FaceNaviOrangeRobot" Value="I saw it!!" Face="NaviOrangeRobot" Mono="False" />
  <!-- そなたの探しておるものはズバリ「Ｋｅｙ．ｂａｔ」じゃ！ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue14FaceNaviOrangeRobot" Value="You’re definitely looking for a,”Key.bat”!" Face="NaviOrangeRobot" Mono="False" />
  <!-- そしてそれは････子供がたくさんいる所で、本のデータが多い電脳に存在するぞい！ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue15FaceNaviOrangeRobot" Value="And you’ll find it....,In a place with many children，,in a comp with many books!" Face="NaviOrangeRobot" Mono="False" />
  <!-- わしにわかるのはそのくらいじゃあとは自分の頭と足で考えるとよかろうて -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage1Dialogue16FaceNaviOrangeRobot" Value="That’s about all I know.,You will have to figure out,the rest on your own." Face="NaviOrangeRobot" Mono="False" />
  <!-- リペアで回復しないと転送病で消えてしまうんじゃよ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue1FaceNaviOrangeRobot" Value="I need to heal every so often，,or I’ll succumb to,Transfer Sickness." Face="NaviOrangeRobot" Mono="False" />
  <!-- 転送病っていうのはなワープ移動の際にほんの少しデータが劣化してしまうんじゃ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue2FaceNaviOrangeRobot" Value="Transfer Sickness is data,deterioratation caused by,using warps." Face="NaviOrangeRobot" Mono="False" />
  <!-- プラグアウトすれば治るんじゃがワシのような野良ナビはやがてデータが壊れてしまうんじゃよ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue3FaceNaviOrangeRobot" Value="Jacking out will cure it，but,rural Navis like me will fall,apart before long." Face="NaviOrangeRobot" Mono="False" />
  <!-- じゃから　定期的にリペアで回復する必要があるのじゃ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue4FaceNaviOrangeRobot" Value="Because of that，I need to heal,regularly with Recov chips." Face="NaviOrangeRobot" Mono="False" />
  <!-- ちなみに検索ソフト「千里眼」とは電脳世界全域を検索できるスゴイソフトなんじゃよ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue5FaceNaviOrangeRobot" Value="By the way，the ”ESP” search,engine can search the entire,cyberworld." Face="NaviOrangeRobot" Mono="False" />
  <!-- じゃが、検索結果はアイマイで正確な位置までは特定はできなくてのう -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue6FaceNaviOrangeRobot" Value="However，the results are,vague and don’t pinpoint,the exact location." Face="NaviOrangeRobot" Mono="False" />
  <!-- 開発中止になってしまったというわけなんじゃよ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue7FaceNaviOrangeRobot" Value="That’s why it was cancelled,during development." Face="NaviOrangeRobot" Mono="False" />
  <!-- それで　失敗作のワシは捨てられたってわけじゃハッハッハ！ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage3Dialogue8FaceNaviOrangeRobot" Value="And so，they gave up on me,as a failed project.,Hahaha!" Face="NaviOrangeRobot" Mono="False" />
  <!-- ここにいた爺さんナビ？ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage5Dialogue1FaceNaviOrangeMale" Value="The old Navi that was here?" Face="NaviOrangeMale" Mono="False" />
  <!-- 元気になったからって言ってエイエンスクエアに向かっていったよ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage5Dialogue2FaceNaviOrangeMale" Value="I hear he got better and went,back to Eien Square." Face="NaviOrangeMale" Mono="False" />
  <!-- エイエンスクエアのあるエイエンエリアにはこのエリアの右下の方から行けるよ -->
  <Dialogue Key="Map.CityNet3Chapter3SageStage5Dialogue3FaceNaviOrangeMale" Value="You can go to Eien Area,from the bottom-right part,of this Area." Face="NaviOrangeMale" Mono="False" />
  <!-- 久しぶりに友人のナビに会ったらバージョンアップしてイケメンになってた････ -->
  <Dialogue Key="Map.CityNet3Chapter4Navi1Stage1Dialogue1FaceNaviGreenRobot" Value="An old friend’s Navi got,upgraded to a new,handsome version." Face="NaviGreenRobot" Mono="False" />
  <!-- 俺も次世代ナビにバージョンアップしたいなあ -->
  <Dialogue Key="Map.CityNet3Chapter4Navi1Stage1Dialogue2FaceNaviGreenRobot" Value="I wish I could upgrade to the,next-gen look...." Face="NaviGreenRobot" Mono="False" />
  <!-- 最近知ったんだけど草属性の攻撃って無敵が発生しないんだってね -->
  <Dialogue Key="Map.CityNet3Chapter4Navi2Stage1Dialogue1FaceNaviOrangeMale" Value="I just noticed that Leaf element,attacks don’t cause invicibility." Face="NaviOrangeMale" Mono="False" />
  <!-- うまく使えばコンボに使えるかもしれないな -->
  <Dialogue Key="Map.CityNet3Chapter4Navi2Stage1Dialogue2FaceNaviOrangeMale" Value="Use them well and you might,get a good combo." Face="NaviOrangeMale" Mono="False" />
  <!-- ････さてそろそろ引き上げるかな -->
  <Dialogue Key="Map.CityNet3Request12NaviStage1Dialogue1FaceNaviBlueRobot" Value="I suppose I should close up，,then...." Face="NaviBlueRobot" Mono="False" />
  <!-- ん？行商人とは私のことだが････ -->
  <Dialogue Key="Map.CityNet3Request12NaviStage1Dialogue2FaceNaviBlueRobot" Value="Hm? Do I sell chips?,Any other day，you would be,right." Face="NaviBlueRobot" Mono="False" />
  <!-- 商品がひとつ残らず品切れになったから店じまいだよ -->
  <Dialogue Key="Map.CityNet3Request12NaviStage1Dialogue3FaceNaviBlueRobot" Value="Today I’m all sold out，I’m,afraid." Face="NaviBlueRobot" Mono="False" />
  <!-- 「ブラインドリーフ　Ｐ」が欲しかったって？残念だったねえ -->
  <Dialogue Key="Map.CityNet3Request12NaviStage1Dialogue4FaceNaviBlueRobot" Value="Looking for a BlndLeaf P?,You’re not the first person to,come around asking for one." Face="NaviBlueRobot" Mono="False" />
  <!-- その商品なら一人の客が５枚すべて買い占めていったよ -->
  <Dialogue Key="Map.CityNet3Request12NaviStage1Dialogue5FaceNaviBlueRobot" Value="Five people today alone were,interested in that specific chip." Face="NaviBlueRobot" Mono="False" />
  <!-- なんでも外国のウラスクエアで人気のチップなんだってね -->
  <Dialogue Key="Map.CityNet3Request12NaviStage1Dialogue6FaceNaviBlueRobot" Value="It seems that it’s a favorite,in the UnderSquares of the,foreign Net." Face="NaviBlueRobot" Mono="False" />
  <!-- 次に店を出す時は危険を覚悟でウラに出店も考えてみるか････ -->
  <Dialogue Key="Map.CityNet3Request12NaviStage1Dialogue7FaceNaviBlueRobot" Value="If you’ve got the guts，it’d be,pretty profitable to set up shop,over there." Face="NaviBlueRobot" Mono="False" />
  <!-- 君が欲しがった商品なら一人の客が５枚すべて買い占めていったよ -->
  <Dialogue Key="Map.CityNet3Request12NaviStage3Dialogue1FaceNaviBlueRobot" Value="I’m all out of BlndLeaf P.,Your best bet might be finding,someone who bought one." Face="NaviBlueRobot" Mono="False" />
  <!-- なんでも外国のウラスクエアで人気のチップなんだってね -->
  <Dialogue Key="Map.CityNet3Request12NaviStage3Dialogue2FaceNaviBlueRobot" Value="It’s quite popular in foreign,UnderSquares." Face="NaviBlueRobot" Mono="False" />
  <!-- 次に店を出す時は危険を覚悟でウラに出店も考えてみるか････ -->
  <Dialogue Key="Map.CityNet3Request12NaviStage3Dialogue3FaceNaviBlueRobot" Value="If it were anywhere else，I’d,think about setting up a shop,over there." Face="NaviBlueRobot" Mono="False" />
  <!-- あらぁ　あなたの顔ってすごくおいしそう！ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage1Dialogue1FaceNaviBlackRobe" Value="Your face! It looks delicious!,I want to eat it!" Face="NaviBlackRobe" Mono="False" />
  <!-- ？？？？ -->
  <Text Key="Map.CityNet3BlackRobe3Battle1Enemy1" Value="????" />
  <!-- ････ふうシティエリアの連中はこれで全部みたいね -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage3Dialogue1FaceShanghai" Value="....Looks like that’s everyone in,City Area." Face="Shanghai" Mono="False" />
  <!-- アリス　電話よ！鈴仙から！ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue1FaceShanghai" Value="Alice，a phone call!,From Reisen!" Face="Shanghai" Mono="False" />
  <!-- 鈴仙！言われたエリアの黒いナビは全部倒したわよ！ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue2FaceAlice" Value="Reisen! I’ve taken out the black,Navis in the Areas!" Face="Alice" Mono="False" />
  <!-- こちらでも確認した助かった礼を言う -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue3FaceReisen" Value="Confirmed. Your assistance is,appreciated." Face="Reisen" Mono="False" />
  <!-- どうやら連中はウラインターネットの深部から各地へと移動しているようだ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue4FaceReisen" Value="Reports are coming in that they,emerged from the deepest,parts of the Undernet." Face="Reisen" Mono="False" />
  <!-- ウラインターネットにも多数の交戦を確認している以上ウラの住人というわけでもない -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue5FaceReisen" Value="Sources in the Undernet claim,that they are not inhabitants，,but rather appeared suddenly." Face="Reisen" Mono="False" />
  <!-- 続けてで悪いが連中の正体を探るために調査を頼めるか？ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue6FaceReisen" Value="It pains me to ask，but can you,investigate their origins?" Face="Reisen" Mono="False" />
  <!-- ウラインターネットの深部････わかったわ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue7FaceShanghai" Value="The Deep Undernet...." Face="Shanghai" Mono="False" />
  <!-- いいの？　シャンハイ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue8FaceAlice" Value="Are you alright，Shanghai?" Face="Alice" Mono="False" />
  <!-- あいつらの態度にか･な･り　ムカついてるのよね -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue9FaceShanghai" Value="It’s nothing.,Just the nerve of those guys!" Face="Shanghai" Mono="False" />
  <!-- 根絶やしにできるならなんだってしてやるわよ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue10FaceShanghai" Value="We’ll wipe them out!" Face="Shanghai" Mono="False" />
  <!-- ありがたい -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue11FaceReisen" Value="Our thanks." Face="Reisen" Mono="False" />
  <!-- 報酬が欲しければ後々ネットエージェントに請求してくれ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue12FaceReisen" Value="If you want a reward，contact,the NetAgents after this is over." Face="Reisen" Mono="False" />
  <!-- わかったわそれをもらうにもまずは調査に行かないとね！ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue13FaceShanghai" Value="We’ll hunt these guys down in,no time flat!" Face="Shanghai" Mono="False" />
  <!-- でもウラインターネットの深部って大丈夫かな？ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue14FaceAlice" Value="....Will we be okay in the Deep,Undernet?" Face="Alice" Mono="False" />
  <!-- 何を今更！あたしとアリスならどこへ行っても無敵よ！ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue15FaceShanghai" Value="What are you talking about?,We’re invincible wherever we,go!" Face="Shanghai" Mono="False" />
  <!-- ････そうね！いきましょうウラインターネットに！ -->
  <Dialogue Key="Map.CityNet3BlackRobe3Stage5Dialogue16FaceAlice" Value="....Yeah!,To the Deep Undernet!" Face="Alice" Mono="False" />
  <!-- おや　あなたが依頼を受けてくれた人ですか？ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue1FaceNaviOrangeMale" Value="Oh，are you the one who took,my request?" Face="NaviOrangeMale" Mono="False" />
  <!-- 私はＳＮＳ「キズナフレンド」の利用者なのですが最近困っているんです -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue2FaceNaviOrangeMale" Value="I use the ”Friend Link”,social media network，but,I’m frustrated with it." Face="NaviOrangeMale" Mono="False" />
  <!-- というのも最近代理で来た管理人がひどい守銭奴で････ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue3FaceNaviOrangeMale" Value="The new replacement admin,is a filthy cheapskate...." Face="NaviOrangeMale" Mono="False" />
  <!-- かなりの金額を課金しないとろくにサービスを利用することができなくなりました -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue4FaceNaviOrangeMale" Value="The fees are so high now,that we can’t use it anymore." Face="NaviOrangeMale" Mono="False" />
  <!-- （「キズナフレンド」ってあたしの　しもべが運営してるやつ････） -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue5FaceShanghai" Value="(Some of my servants are, admins of Friend Link....)" Face="Shanghai" Mono="False" />
  <!-- なので　この代理管理人をこらしめてほしいんです！お願いします！ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue6FaceNaviOrangeMale" Value="That’s why I’d like you to,punish this bad admin!,Please!" Face="NaviOrangeMale" Mono="False" />
  <!-- ････わかったわ！このあたしにドーンと任せなさい！ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue7FaceShanghai" Value="....Got it!,Just leave it to me!" Face="Shanghai" Mono="False" />
  <!-- おお！　ありがとうございます！これが管理エリアに入れるＰコードです！ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue8FaceNaviOrangeMale" Value="Oh! Thank you!,Now let me give you the,Admin Area’s P-Code!" Face="NaviOrangeMale" Mono="False" />
  <!-- これでシティエリア２にあるセキュリティキューブを解除して管理人のところに入れます -->
  <Dialogue Key="Map.CityNet3Request8NaviStage1Dialogue9FaceNaviOrangeMale" Value="This will unlock the P-Cube to,the Admin Area in City Area 2." Face="NaviOrangeMale" Mono="False" />
  <!-- 管理エリアの場所はシティエリア２ですおねがいしますね -->
  <Dialogue Key="Map.CityNet3Request8NaviStage3Dialogue1FaceNaviOrangeMale" Value="The Admin Area is in,City Area 2.,I’m counting on you!" Face="NaviOrangeMale" Mono="False" />
  <!-- おお！　代理管理人をこらしめてくれたんですね！ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage5Dialogue1FaceNaviOrangeMale" Value="Oh! You were actually able to,punish the bad replacement,admin!" Face="NaviOrangeMale" Mono="False" />
  <!-- ありがとうございます！これで気持よく利用できるようになるでしょう！ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage5Dialogue2FaceNaviOrangeMale" Value="Thank you!,Now we should be able to,use Friend Link again!" Face="NaviOrangeMale" Mono="False" />
  <!-- それでは　これが報酬です！ -->
  <Dialogue Key="Map.CityNet3Request8NaviStage5Dialogue3FaceNaviOrangeMale" Value="And now，here’s your reward!" Face="NaviOrangeMale" Mono="False" />
  <!-- 「キズナフレンド」は友達との貴重な連絡手段でもありますからね -->
  <Dialogue Key="Map.CityNet3Request8NaviStage7Dialogue1FaceNaviOrangeMale" Value="Friend Link is an invaluable,way to stay in touch with,friends." Face="NaviOrangeMale" Mono="False" />
  <!-- 使えなくなったら大変なことになるところでしたよ -->
