  <Dialogue Key="Map.AirCleaner3Sprite1Stage1Dialogue1FaceSprite" Value="AN OVERWHELMING,AMOUNT OF GAS,APPEARED!" Face="Sprite" Mono="False" />
  <Dialogue Key="Map.AirCleaner3Sprite1Stage1Dialogue2FaceSprite" Value="PLEASE BE CAREFUL AS,YOU PROCEED!" Face="Sprite" Mono="False" />
  <Dialogue Key="Map.AirCleaner3Sprite1Stage3Dialogue1FaceSprite" Value="WE’RE SAVED!" Face="Sprite" Mono="False" />
  <Dialogue Key="Map.AirCleaner3Sprite2Stage1Dialogue1FaceSprite" Value="IF YOU PASS OUT，THE,RESCUE PROGRAM WILL,BRING YOU BACK HERE!" Face="Sprite" Mono="False" />
  <Dialogue Key="Map.AirCleaner3Sprite2Stage1Dialogue2FaceSprite" Value="PLEASE BE REALLY REALLY,REALLY CAREFUL AROUND,THE GAS!" Face="Sprite" Mono="False" />
  <Dialogue Key="Map.AirCleaner3Sprite3Stage1Dialogue1FaceSprite" Value="FROM THIS POINT ON，THE,RESCUE PROGRAM WILL,BRING YOU BACK HERE!" Face="Sprite" Mono="False" />
