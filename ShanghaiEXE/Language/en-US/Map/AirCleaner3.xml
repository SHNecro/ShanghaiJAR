  <Dialogue Key="Map.AirCleaner3Sprite4Stage1Dialogue2FaceSprite" Value="MAYBE I SHOULD THINK,ABOUT HOW TO GET,BACK OUT TOO!" Face="Sprite" Mono="False" />
  <Dialogue Key="Map.AirCleaner3Sprite4Stage3Dialogue1FaceSprite" Value="NOW THAT THE GAS IS,GONE，IT’S EASY TO LIVE,HERE AGAIN!" Face="Sprite" Mono="False" />
