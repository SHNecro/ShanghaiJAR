  <Dialogue Key="Map.Class2nen1kumiClassClassCollectionStage1Dialogue1FaceNone" Value="This is the class reading,material." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiClassClassCollectionStage1Dialogue2FaceNone" Value="There are famous fantasy,stories and study manga,available in Japanese." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiClassClassCollectionStage1Dialogue3FaceNone" Value="There’s also an intense war,manga here，for some reason." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiClassClassCollectionStage1Dialogue4FaceNone" Value="It might traumatize young,minds that read it." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiClassClassCollectionStage1Dialogue5FaceNone" Value="....Something is caught,between the books." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiThats’ItStage1Dialogue1FaceNone" Value="It’s a graph of something,changing price." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiThats’ItStage1Dialogue2FaceNone" Value="Will we be using this in,civics class?" Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiBlackboardStage1Dialogue1FaceNone" Value="This classroom has an,interactive blackboard." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiBlackboardStage1Dialogue2FaceNone" Value="We use electronic chalk so it,doesn’t get dirty.,I’m glad it’s easy to clean." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiBlackboardStage1Dialogue3FaceNone" Value="We can also jack in to show,projects we made on our,computers." Face="None" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiPostGameBystander1Dialogue1" Value="I wonder what we’re going to,learn before summer break?" Face="MaleChild" Mono="False" />
  <Dialogue Key="Map.Class2nen1kumiPostGameBystander2Dialogue1" Value="There’s actually a boy in this,grade that has never been to,class!" Face="FemaleChild" Mono="False" />
