  <Dialogue Key="Map.GenUniversity3roomChair1Stage1Dialogue1FaceNone" Value="It’s a lab chair." Face="None" Mono="False" />
  <Dialogue Key="Map.GenUniversity3roomChair1Stage1Dialogue2FaceNone" Value="It looks nice，but it rattles,when I touch it.,I wonder how old it is." Face="None" Mono="False" />
  <Dialogue Key="Map.GenUniversity3roomChairShadow1Stage1Dialogue1FaceNone" Value="It’s Dr. Morichika’s chair." Face="None" Mono="False" />
