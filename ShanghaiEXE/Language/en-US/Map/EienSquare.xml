<?xml version="1.0" encoding="utf-8"?>
<data>
  <!-- インターネット　エイエンスクエア -->
  <Text Key="Map.EienSquareName" Value="Eien Square" />
  <!-- このスクエアは床がウサちゃんの形になってるのよ！チョ〜かわいい！ -->
  <Dialogue Key="Map.EienSquareChapter3GirlNavi1Stage1Dialogue1FaceNaviPinkFemale" Value="This Square is shaped like a,rabbit! It’s sooooo cute〜!" Face="NaviPinkFemale" Mono="False" />
  <!-- エイエンエリアのどこかから恐ろしいインターネットエリアに行けるらしい････ -->
  <Dialogue Key="Map.EienSquareChapter3ManNavi1Stage1Dialogue1FaceNaviGreenRobot" Value="I hear a terrifying Area lies,just beyond Eien Area...." Face="NaviGreenRobot" Mono="False" />
  <!-- その名もウラインターネットとか････怖いなぁー -->
  <Dialogue Key="Map.EienSquareChapter3ManNavi1Stage1Dialogue2FaceNaviGreenRobot" Value="It’s called the Undernet....,Scary，huh?" Face="NaviGreenRobot" Mono="False" />
  <!-- おばけのウィルスにペロペロされると目が見えなくなっちゃうの -->
  <Dialogue Key="Map.EienSquareChapter3ChildNavi1Stage1Dialogue1FaceNaviChild" Value="When a ghost virus licks you，,you’ll go blind." Face="NaviChild" Mono="False" />
  <!-- しかもちょっとだけ動けなくなるから一気にピンチになっちゃう････ -->
  <Dialogue Key="Map.EienSquareChapter3ChildNavi1Stage1Dialogue2FaceNaviChild" Value="You won’t be able to move for,a while either，so you’ll be in,a heap of trouble...." Face="NaviChild" Mono="False" />
  <!-- ハーイ！ワタシ　東ヨーリカのインゲレスから来ましたー！ -->
  <Dialogue Key="Map.EienSquareChapter3ManNavi2Stage1Dialogue1FaceNaviGreenRobot" Value="Hiii!,I came all the way from,Engelles in East Eurica!" Face="NaviGreenRobot" Mono="False" />
  <!-- 日本トッテモ平和でいいとこデスネー！ -->
  <Dialogue Key="Map.EienSquareChapter3ManNavi2Stage1Dialogue2FaceNaviGreenRobot" Value="The best thing about Japan,is that it’s very peaceful!" Face="NaviGreenRobot" Mono="False" />
  <!-- ぬ？おお　昨日の嬢ちゃんじゃないか -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue1FaceNaviOrangeRobot" Value="Hm?,Oh，if it isn’t the lass from,yesterday." Face="NaviOrangeRobot" Mono="False" />
  <!-- 探しものは見つかったかの？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue2FaceNaviOrangeRobot" Value="Did you find what you were,looking for?" Face="NaviOrangeRobot" Mono="False" />
  <!-- ああ　その件はありがと今日はまた探しものをしてほしいのよ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue3FaceShanghai" Value="Yeah. Thanks for your help.,Today，I’m looking for,something different." Face="Shanghai" Mono="False" />
  <!-- はっはっは昨日の恩があるし良いじゃろう -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue4FaceNaviOrangeRobot" Value="Ha ha ha....,I owe you a favor from,yesterday，so I’ll help." Face="NaviOrangeRobot" Mono="False" />
  <!-- えっとねウィルスを飼育する方法を探しているのよね -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue5FaceShanghai" Value="Let’s see....,I’m looking for ways to,tame viruses." Face="Shanghai" Mono="False" />
  <!-- バクゼンとした探しものじゃの -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue6FaceNaviOrangeRobot" Value="For something like that，one,does not merely find the path.,One must also walk it." Face="NaviOrangeRobot" Mono="False" />
  <!-- むむむむむ・・・・ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue7FaceNaviOrangeRobot" Value="Hmmmmmm...." Face="NaviOrangeRobot" Mono="False" />
  <!-- カーーーーッ！ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue8FaceNaviOrangeRobot" Value="Ding!" Face="NaviOrangeRobot" Mono="False" />
  <!-- むむっ！？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue9FaceNaviOrangeRobot" Value="Hmm!?" Face="NaviOrangeRobot" Mono="False" />
  <!-- どうしたの　爺さん？見つからなかった？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue10FaceShanghai" Value="What’s wrong，Sage?,Couldn’t find it?" Face="Shanghai" Mono="False" />
  <!-- いや　ぼんやりと浮かんだがどうやらウラインターネットのようじゃ････ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue11FaceNaviOrangeRobot" Value="No，I found it.,I can feel its presence in the,Undernet，but...." Face="NaviOrangeRobot" Mono="False" />
  <!-- ウラインターネット？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue12FaceShanghai" Value="The Undernet?" Face="Shanghai" Mono="False" />
  <!-- ウラインターネットとは恐ろしいインターネットエリアなんじゃよ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue13FaceNaviOrangeRobot" Value="The Undernet is a horrid part,of the Internet." Face="NaviOrangeRobot" Mono="False" />
  <!-- うかつに行くのは自殺行為かもしれんぞ？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue14FaceNaviOrangeRobot" Value="It’d be suicidal to go there，,you know?" Face="NaviOrangeRobot" Mono="False" />
  <!-- ウラ････ねえ？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue15FaceShanghai" Value="The Undernet，huh....?" Face="Shanghai" Mono="False" />
  <!-- まあ　あたしなら大丈夫でしょ爺さん　ありがとねー -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue16FaceShanghai" Value="Well，I’m me，so it’ll be fine.,Thanks，Sage." Face="Shanghai" Mono="False" />
  <!-- そ　そうか････ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue17FaceNaviOrangeRobot" Value="Oh，I see...." Face="NaviOrangeRobot" Mono="False" />
  <!-- ウラインターネットはエイエンエリア３から行けるが気をつけるんじゃぞ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage1Dialogue18FaceNaviOrangeRobot" Value="You can enter the Undernet,from Eien Area 3，but please,be careful." Face="NaviOrangeRobot" Mono="False" />
  <!-- ウラインターネットはエイエンエリア３から行けるが気をつけるんじゃぞ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage3Dialogue1FaceNaviOrangeRobot" Value="You can enter the Undernet,from Eien Area 3，but please,be careful." Face="NaviOrangeRobot" Mono="False" />
  <!-- おお！ごぶさたじゃのう！ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue1FaceNaviOrangeRobot" Value="....Oh! Excuse me，young lady!" Face="NaviOrangeRobot" Mono="False" />
  <!-- 元気そうね　じいさん -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue2FaceShanghai" Value="No problem，gramps." Face="Shanghai" Mono="False" />
  <!-- 早速だけど仕事の依頼よ「転生プログラム」を探して欲しいの -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue3FaceShanghai" Value="I need you to look for a....,”Reincarnation Program”." Face="Shanghai" Mono="False" />
  <!-- ホッホッホッ！お安い御用じゃ！ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue4FaceNaviOrangeRobot" Value="No problem!" Face="NaviOrangeRobot" Mono="False" />
  <!-- むむむむむ・・・・ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue5FaceNaviOrangeRobot" Value="Mmmm-mmm-mmm...." Face="NaviOrangeRobot" Mono="False" />
  <!-- カーーーーッ！ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue6FaceNaviOrangeRobot" Value="Aah!" Face="NaviOrangeRobot" Mono="False" />
  <!-- むむっ！？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue7FaceNaviOrangeRobot" Value="Hmm?!" Face="NaviOrangeRobot" Mono="False" />
  <!-- どうしたの？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue8FaceShanghai" Value="What’s going on?" Face="Shanghai" Mono="False" />
  <!-- 見つけたには見つけたのじゃが････ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue9FaceNaviOrangeRobot" Value="I found it，but...." Face="NaviOrangeRobot" Mono="False" />
  <!-- なんと　その場所は大型輸送船セプテニック号の電脳世界の中なのじゃ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue10FaceNaviOrangeRobot" Value="It’s in the cyber-world of a,big cargo ship，the Septanic." Face="NaviOrangeRobot" Mono="False" />
  <!-- ということは乗船券を手に入れないといけないのね -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue11FaceAlice" Value="We’ll need to ask if we can,board." Face="Alice" Mono="False" />
  <!-- ････待って今セプテニック号って言ったわよね？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue12FaceShanghai" Value="Wait a second....,You said the Septanic?" Face="Shanghai" Mono="False" />
  <!-- それって数年前に謎の事故で沈んだ船じゃなかったっけ？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue13FaceShanghai" Value="Wasn’t it that ship that sank in a,mysterious accident a few,years back?" Face="Shanghai" Mono="False" />
  <!-- 沈没船ってこと････？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue14FaceAlice" Value="It’s a sunken ship?" Face="Alice" Mono="False" />
  <!-- 航海中に船のメインシステムが暴走して沈んだと伝えられる謎の多い船じゃ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue15FaceNaviOrangeRobot" Value="The final reports told of a,malfunction in the ship’s control,systems...." Face="NaviOrangeRobot" Mono="False" />
  <!-- アクセスする方法はあるの？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue16FaceShanghai" Value="How are we gonna get there?" Face="Shanghai" Mono="False" />
  <!-- 最近の客船なら必ずついているサルベージプログラムを使うとよいじゃろう -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue17FaceNaviOrangeRobot" Value="As luck has it，there’s a,salvage program on a ship,that just launched." Face="NaviOrangeRobot" Mono="False" />
  <!-- 海底ケーブルの電脳世界や漁礁の電脳にアクセスする装置だっけ？ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue18FaceShanghai" Value="The kind that uses a submarine,to run connections to cyber-,worlds?" Face="Shanghai" Mono="False" />
  <!-- 船の中でも海が見える場所にあるはずじゃ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue19FaceNaviOrangeRobot" Value="You should be able to connect,from a place with a view of the,sea." Face="NaviOrangeRobot" Mono="False" />
  <!-- そのプログラムで座標を求められたらＸ−９８Y−０５と入力するのじゃ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue20FaceNaviOrangeRobot" Value="If it asks for coordinates，punch,in X-98，Y-05." Face="NaviOrangeRobot" Mono="False" />
  <!-- わかったわありがとう！ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue21FaceShanghai" Value="Alright，thanks!" Face="Shanghai" Mono="False" />
  <!-- くれぐれも達者でな -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage5Dialogue22FaceNaviOrangeRobot" Value="Good talking to you!" Face="NaviOrangeRobot" Mono="False" />
  <!-- 最近の客船なら必ずついているサルベージプログラムを使うとよいじゃろう -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage7Dialogue1FaceNaviOrangeRobot" Value="As luck has it，there’s a,salvage program on a ship,that just launched." Face="NaviOrangeRobot" Mono="False" />
  <!-- 船の中でも海が見える場所にあるはずじゃ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage7Dialogue2FaceNaviOrangeRobot" Value="You should be able to connect,from a place with a view of the,sea." Face="NaviOrangeRobot" Mono="False" />
  <!-- そのプログラムで座標を求められたらＸ−９８Y−０５と入力するのじゃ -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage7Dialogue3FaceNaviOrangeRobot" Value="If it asks for coordinates，punch,in X-98，Y-05." Face="NaviOrangeRobot" Mono="False" />
  <!-- くれぐれも達者でな -->
  <Dialogue Key="Map.EienSquareChapter3SageOldManStage7Dialogue4FaceNaviOrangeRobot" Value="Good talking to you!" Face="NaviOrangeRobot" Mono="False" />
  <!-- ボーダーコンツェルン主催Ｎ１リーグのエントリーはこちらで受け付けていまーす！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage1Dialogue1FaceNaviPinkFemale" Value="I’m accepting entries for,the N1 League，hosted by,Border Concern!" Face="NaviPinkFemale" Mono="False" />
  <!-- 参加希望の方ですか？ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage1Dialogue2QuestionFaceNaviPinkFemale" Value="Would you like to enter?" Face="NaviPinkFemale" Mono="False" />
  <!-- はいいいえ -->
  <Text Key="Map.EienSquareChapter4N1ReceptionStage1Dialogue2Options" Value="Yes,No" />
  <!-- エントリーしたいんだけどいいかしら？ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage1Dialogue3FaceShanghai" Value="I’d like to enter.,Is that alright?" Face="Shanghai" Mono="False" />
  <!-- はい　大丈夫です！ではこちらにお名前を -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage1Dialogue4FaceNaviPinkFemale" Value="Sure! No problem!,Just sign here...." Face="NaviPinkFemale" Mono="False" />
  <!-- ・・・・・・・・はいそれで大丈夫です！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage1Dialogue5FaceNaviPinkFemale" Value="........There!,You’re good to go!" Face="NaviPinkFemale" Mono="False" />
  <!-- それでは間もなく予選が始まりますのであちらの方へどうぞ！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage1Dialogue6FaceNaviPinkFemale" Value="The qualifiers will be starting,soon，so you better head on,over there!" Face="NaviPinkFemale" Mono="False" />
  <!-- シャンハイちゃん！頑張ろうな！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage3Dialogue1FaceSpannerMan" Value="Hey，Shanghai!,Let’s rock this!" Face="SpannerMan" Mono="False" />
  <!-- 優勝はあたしがいただくんだからね -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage3Dialogue2FaceShanghai" Value="I’ll just help myself to the title，,if you don’t mind!" Face="Shanghai" Mono="False" />
  <!-- えー････コホン -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage5Dialogue1FaceNaviBlueRobot" Value="Ehm.... Ahem!" Face="NaviBlueRobot" Mono="False" />
  <!-- これより　Ｎ１リーグの予選を開始いたします -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage5Dialogue2FaceNaviBlueRobot" Value="We will now begin the,N1 League qualifiers!" Face="NaviBlueRobot" Mono="False" />
  <!-- 予選の内容は簡単エイエンエリア内にＮ１リーグのスタッフが４人います -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage5Dialogue3FaceNaviBlueRobot" Value="The rules are simple.,Four N1 staff members await,you in the Eien Areas." Face="NaviBlueRobot" Mono="False" />
  <!-- そのスタッフとバトルをして全員に勝利すれば予選突破となります -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage5Dialogue4FaceNaviBlueRobot" Value="If you manage to defeat them,all，you’ll move on to the main,event." Face="NaviBlueRobot" Mono="False" />
  <!-- なお　予選突破者が規定数を満たした時点で予選は終了となります -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage5Dialogue5FaceNaviBlueRobot" Value="Also，the qualifiers will end,once a certain number of you,qualify." Face="NaviBlueRobot" Mono="False" />
  <!-- へー　簡単そうな予選じゃないの -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage7Dialogue1FaceShanghai" Value="That’s it?,Seems a bit easy for a,qualifying round，huh?" Face="Shanghai" Mono="False" />
  <!-- そうね　最近の戦いでバトルチップも充実してるし････ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage7Dialogue2FaceAlice" Value="True，and we’ve been finding,better Battle Chips lately...." Face="Alice" Mono="False" />
  <!-- ただし　当大会はネットバトラーの技能向上を目的としており -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage9Dialogue1FaceNaviBlueRobot" Value="However，this tournament is,also meant to improve your,NetBattling technique." Face="NaviBlueRobot" Mono="False" />
  <!-- そのため予選はこちらで用意したチップフォルダで行ってもらいます -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage9Dialogue2FaceNaviBlueRobot" Value="For that reason，you must use,this prepared folder during the,qualifiers." Face="NaviBlueRobot" Mono="False" />
  <!-- へ？ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage9Dialogue3FaceShanghai" Value="Huh?" Face="Shanghai" Mono="False" />
  <!-- ガチャ　ガチャン -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage9Dialogue4FaceNone" Value="KACHUNK!,THUNK!" Face="None" Mono="False" />
  <!-- ああっ！　チップフォルダが！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage9Dialogue5FaceShanghai" Value="Aaah! My chip folder!" Face="Shanghai" Mono="False" />
  <!-- ただいま皆様のチップフォルダを「リーグフォルダ」に固定しました -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage9Dialogue6FaceNaviBlueRobot" Value="I just locked everyone’s chip,folders to the ”N1 Folder”." Face="NaviBlueRobot" Mono="False" />
  <!-- へっ！　それくらいどうってことないぜ！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage9Dialogue7FaceSpannerMan" Value="Heh!,Like that’ll stop me!" Face="SpannerMan" Mono="False" />
  <!-- それでは　予選開始！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage11Dialogue1FaceNaviBlueRobot" Value="And with that，let the,N1 League qualifiers begin!" Face="NaviBlueRobot" Mono="False" />
  <!-- シャンハイ････大丈夫？ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage13Dialogue1FaceAlice" Value="Shanghai....?,You okay?" Face="Alice" Mono="False" />
  <!-- なんのこれしき！豪華客船のためならへっちゃらよ！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage13Dialogue2FaceShanghai" Value="This is nothing! Just watch!,We’re gonna win that cruise!" Face="Shanghai" Mono="False" />
  <!-- あはは　頼もしいわね････ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage13Dialogue3FaceAlice" Value="Ahaha....,At least you’re confident...." Face="Alice" Mono="False" />
  <!-- それじゃあ大会のスタッフを探しに行きましょうか -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage13Dialogue4FaceAlice" Value="Well，let’s go look for the,tournament staff." Face="Alice" Mono="False" />
  <!-- エイエンエリア内に４人いるんだったわよね -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage13Dialogue5FaceShanghai" Value="He said there were four of,them in the Eien Areas." Face="Shanghai" Mono="False" />
  <!-- よっし　じゃあパパっと終わらせるわよ！ -->
  <Dialogue Key="Map.EienSquareChapter4N1ReceptionStage13Dialogue6FaceShanghai" Value="Alright! Let’s do this!" Face="Shanghai" Mono="False" />
  <!-- エイエンエリアのどこかにいるスタッフ４人とバトルしたらここに戻ってきてください -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage1Dialogue1FaceNaviBlueRobot" Value="Please return here once,you defeat all four of the,N1 staff members." Face="NaviBlueRobot" Mono="False" />
  <!-- 勝ったわよ！ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage3Dialogue1FaceShanghai" Value="I won!" Face="Shanghai" Mono="False" />
  <!-- ････はい！シャンハイさん　予選突破おめでとうございます -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage3Dialogue2FaceNaviBlueRobot2" Value="....Okay! Congratulations,for qualifying，Shanghai!" Face="NaviBlueRobot2" Mono="False" />
  <!-- やったわね　シャンハイ！ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage3Dialogue3FaceAlice" Value="We did it，Shanghai!" Face="Alice" Mono="False" />
  <!-- はん！　当然よ！ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage3Dialogue4FaceShanghai" Value="Hmph! Naturally!" Face="Shanghai" Mono="False" />
  <!-- おーい！シャンハイちゃーん！ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage3Dialogue5FaceSpannerMan" Value="Yooo! Shanghai!" Face="SpannerMan" Mono="False" />
  <!-- あら　その様子だとスパナマンも予選突破したみたいね -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage5Dialogue1FaceShanghai" Value="Oh，looks like you qualified,too." Face="Shanghai" Mono="False" />
  <!-- あたぼうよ！これくらい　ヘでもねえぜ！ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage5Dialogue2FaceSpannerMan" Value="You know it!,This is nothin’ for a Navi,of my caliber!" Face="SpannerMan" Mono="False" />
  <!-- 二人揃って予選突破したんだお互い本戦頑張ろうな！ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage5Dialogue3FaceSpannerMan" Value="We both qualified!,Let’s do our best at the,main event!" Face="SpannerMan" Mono="False" />
  <!-- 本戦で相手になったらボコボコにしてあげるから覚悟してなさいよ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage5Dialogue4FaceShanghai" Value="If you end up fighting me，,you better be ready for a,beating." Face="Shanghai" Mono="False" />
  <!-- ああ････そうだな････ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage5Dialogue5FaceSpannerMan" Value="Oh.... Right...." Face="SpannerMan" Mono="False" />
  <!-- ････ん？ -->
  <Dialogue Key="Map.EienSquareChapter4ModeratorN1Stage5Dialogue6FaceTsubakiNekku" Value="....Hm?" Face="TsubakiNekku" Mono="False" />
  <!-- スパナマン　そろそろプラグアウトしないと -->
