  <Dialogue Key="Map.GenUniversityChapter8RoboDupeStage1Dialogue2FaceNone" Value="It looks out of place，parked,where students usually hang,out." Face="None" Mono="False" />
  <!-- どこまで乗りますか？ -->
  <Dialogue Key="Map.GenUniversityToTheStationStage1Dialogue1QuestionFaceNone" Value="Ride the metro to where?" Face="None" Mono="False" />
  <!-- 幻想町乗らない -->
  <Text Key="Map.GenUniversityToTheStationStage1Dialogue1Options" Value="Genso Town,Leave" />
  <!-- どこまで乗りますか？ -->
  <Dialogue Key="Map.GenUniversityToTheStationStage3Dialogue1FaceNone" Value="Ride the metro to where?" Face="None" Mono="False" />
  <!--  -->
  <Dialogue Key="Map.GenUniversityToTheStationStage3Dialogue2QuestionFaceNone" Value="" Face="None" Mono="False" />
  <!-- 幻想町中央街西区乗らない -->
  <Text Key="Map.GenUniversityToTheStationStage3Dialogue2Options" Value="Genso Town,Central City,Leave" />
  <!-- どこまで乗りますか？ -->
  <Dialogue Key="Map.GenUniversityToTheStationStage5Dialogue1QuestionFaceNone" Value="Ride the metro to where?" Face="None" Mono="False" />
  <!-- 幻想町中央街西区永遠町２丁目乗らない -->
  <Text Key="Map.GenUniversityToTheStationStage5Dialogue1Options" Value="Genso Town, Central City,Eien Town,Leave" />
  <!-- 大学の前にある噴水だ -->
  <Dialogue Key="Map.GenUniversityWaterSprayUpperLeftStage1Dialogue1FaceNone" Value="There’s a fountain in front of,the University." Face="None" Mono="False" />
  <!-- 周りは涼しいので夏場は絶好の休憩スポットとして学生が集まるようだ -->
  <Dialogue Key="Map.GenUniversityWaterSprayUpperLeftStage1Dialogue2FaceNone" Value="In the summer，students,gather around this refreshing,resting spot." Face="None" Mono="False" />
  <!-- 大学の前にある噴水だ -->
  <Dialogue Key="Map.GenUniversityWaterSprayLowerRightStage1Dialogue1FaceNone" Value="There’s a fountain in front of,the University." Face="None" Mono="False" />
