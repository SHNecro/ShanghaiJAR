  <!-- ････なんとプラグインできる穴を見つけた！ -->
  <Dialogue Key="Map.GenUniversityWaterSprayLowerLeftStage1Dialogue3FaceNone" Value="....Huh?,I found a jack in port!" Face="None" Mono="False" />
  <!-- きっと噴水のコントロールのためにあるのだろう -->
  <Dialogue Key="Map.GenUniversityWaterSprayLowerLeftStage1Dialogue4FaceNone" Value="It must be for the fountain,controls." Face="None" Mono="False" />
  <!-- 閉館していて入れない -->
  <Dialogue Key="Map.GenUniversityToSchoolStage1Dialogue1FaceNone" Value="They’ve already closed.,I can’t go back in." Face="None" Mono="False" />
  <!-- 戦闘テスト -->
  <Dialogue Key="Map.GenUniversityTestSprite1Stage1Dialogue1FaceSprite" Value="Battle Test" Face="Sprite" Mono="False" />
  <!-- 戦闘テスト -->
  <Dialogue Key="Map.GenUniversityTestSprite2Stage1Dialogue1FaceSprite" Value="Battle Test" Face="Sprite" Mono="False" />
  <Dialogue Key="Map.GenUniversityPostGameBystander1Dialogue1" Value="Just a little while ago，an SDF,mobile weapon was parked,right here. I can’t believe it...." Face="FemaleAdult" Mono="False" />
  <Dialogue Key="Map.GenUniversityPostGameBystander2Dialogue1" Value="Dr. Morichika played a big,role in stopping the ROM,crisis...." Face="MaleAdult" Mono="False" />
  <Dialogue Key="Map.GenUniversityPostGameBystander2Dialogue2" Value="Do they give Nobel prizes,for that kind of stuff?" Face="MaleAdult" Mono="False" />
