  <!-- ････どういうこと？ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue9FaceAlice" Value="....What do you mean?" Face="Alice" Mono="False" />
  <!-- 私達はただ降りかかる火の粉を払うしかできないということよ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue10FaceReisen" Value="Man is born free，yet,everywhere he is in chains." Face="Reisen" Mono="False" />
  <!-- そうよね････ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue11FaceAlice" Value="I see...." Face="Alice" Mono="False" />
