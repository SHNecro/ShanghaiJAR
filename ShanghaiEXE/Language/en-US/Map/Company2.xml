  <Text Key="Map.Company2Name" Value="Border Concern HQ Elevator Hallway" />
  <!-- このエレベーターで登れば社長室にいけるはずだ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue1FaceReisen" Value="From this elevator，we will be,able to reach the president’s,office." Face="Reisen" Mono="False" />
  <!-- ････ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue2FaceAlice" Value="...." Face="Alice" Mono="False" />
  <!-- どうした？ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue3FaceReisen" Value="Is something the matter?" Face="Reisen" Mono="False" />
  <!-- 社長ってことは紫さんと会うのよね････ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue4FaceAlice" Value="The president...." Face="Alice" Mono="False" />
  <!-- 私　あの人が悪い人には見えなかったから････ -->
  <Dialogue Key="Map.Company2Chapter7EventsStage1Dialogue5FaceAlice" Value="I didn’t see President Yukari,as a bad person...." Face="Alice" Mono="False" />
