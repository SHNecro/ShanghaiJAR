  <Dialogue Key="Map.YamadaSprite2Stage1Dialogue2FaceSprite" Value="”YAMAZIAN” IS THE,GENERAL TERM FOR A,FAN OF YAMAZE NADU!" Face="Sprite" Mono="False" />
