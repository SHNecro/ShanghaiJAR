  <Dialogue Key="Map.Hospital3fSofaStage1Dialogue1FaceNone" Value="It’s a sofa for people to,sit and wait on." Face="None" Mono="False" />
  <Dialogue Key="Map.Hospital3fSofaStage1Dialogue2FaceNone" Value="It looks soft，but it’s actually,surprisingly stiff." Face="None" Mono="False" />
