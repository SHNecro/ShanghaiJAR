<?xml version="1.0" encoding="utf-8"?>
<data>
  <Text Key="Map.GenUniversity2frinName" Value="Morichika’s Lab" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue1FaceDrMorichika" Value="You all made quite a long trip，,huh?" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue2FaceRika" Value="This lab is amazing!,I’m super excited! I am!" Face="Rika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue3FaceDrMorichika" Value="I’ll let you look around later.,Anyhow...." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue4FaceMari" Value="The Border Concern president,said something interesting,arrived." Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue5FaceDrMorichika" Value="Hahaha!,You already knew?" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue6FaceDrMorichika" Value="She brought me samples of the,new products I helped develop." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue7FaceAlice" Value="How exciting....!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue8FaceDrMorichika" Value="First，the ”AddOn Manager”!" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue9FaceTankMan" Value="AddOns?,Don’t those just add features,to computer applications?" Face="TankMan" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue10FaceDrMorichika" Value="Right，but this one is the,Navi version." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue11FaceDrMorichika" Value="Previous Navi optimizing,programs had quite a steep,learning curve." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue12FaceDrMorichika" Value="But the AddOn Manager,should make things a lot,easier!" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue13FaceShanghai" Value="Neat!,Sounds pretty useful!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue14FaceDrMorichika" Value="That’s what I like to hear!" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue15FaceDrMorichika" Value="Oh，right!,Since you came all the way,out here，why not try it out?" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue16FaceDrMorichika" Value="Here，try installing this data,into your PETs." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari1Stage1Dialogue17FaceDrMorichika" Value="Open the Navi screen and run,the AddOn Manager." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage1Dialogue1FaceAlice" Value="....Hm?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage1Dialogue2FaceShanghai" Value="Everyone’s already looking,around the lab." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage1Dialogue3FaceDrMorichika" Value="If I remember correctly，,didn’t you two want to,ask me something?" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage1Dialogue4FaceShanghai" Value="Oh，right!,We completely forgot!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage3Dialogue1FaceAlice" Value="We wanted to ask if you,wouldn’t mind analyzing,this compressed data." Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage3Dialogue2FaceDrMorichika" Value="Hm. This is quite a rare file,extension. I’ll see what I can,do." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage3Dialogue3FaceAlice" Value="Thank you!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage3Dialogue4FaceShanghai" Value="Alright，Alice! We came all,the way over here，so,let’s take a look around!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2RinnosukeStage5Dialogue1FaceDrMorichika" Value="This data....,Hm，I see...." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari2Stage1Dialogue1FaceMari" Value="I’m not that good at most,software things." Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Rika2Stage1Dialogue1FaceRika" Value="This door looks pretty sturdy!,It even has digital locks!" Face="Rika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Rika2Stage1Dialogue2FaceRika" Value="I want a door with a,digital lock! I do!" Face="Rika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue1FaceRemilia" Value="So this machine is Border,Concern’s new product." Face="Remilia" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue2FaceDrMorichika" Value="It’s a new gaming machine,called the NetBattle Machine." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue3FaceDrMorichika" Value="NetBattles inside are projected,overhead." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue4FaceDrMorichika" Value="Now NetBattles can be shown,to large audiences quite easily." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue5FaceDrMorichika" Value="There’s also one in the,Game Center that’ll be,used for tournaments." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue6FaceShanghai" Value="I see! So now legions of fans,can see my spectacular fights,on the big screen!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue7FaceRemilia" Value="Hohoho!,Sakuya already beat you once!,You don’t stand a chance!" Face="Remilia" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue8FaceSakuya" Value="Milady，there’s no point in,telling her the truth." Face="Sakuya" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue9FaceShanghai" Value="What’d you just say!?,I should knock you out!,Right here，right now!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue10FaceAlice" Value="Shanghai，calm down...." Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue11FaceTankMan" Value="How about they settle this in,the NetBattle Machine?" Face="TankMan" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue12FaceDrMorichika" Value="Hm，that’s a good idea." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue13FaceMarisa" Value="You just wanna see it in action，,don’t ya?" Face="Marisa" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage1Dialogue14FaceDrMorichika" Value="You got me.,Well，let’s get everything ready." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage3Dialogue1FaceDrMorichika" Value="Alright，it’s ready.,Please jack in your Navis." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage5Dialogue1FaceMari" Value="Woah! Cool!" Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage5Dialogue2FaceAlice" Value="Are you sure about this?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage5Dialogue3FaceShanghai" Value="Of course! I’m gonna give her,a taste of her own medicine!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage5Dialogue4FaceRemilia" Value="Still pretending to be a,big shot，are we?" Face="Remilia" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage5Dialogue5FaceDrMorichika" Value="Now then!,Shanghai VS Sakuya!,Battle Routine! Set!" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage5Dialogue6FaceShanghai" Value="Execute!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage5Dialogue7FaceSakuya" Value="Your time is mine....,Let me show you its power!" Face="Sakuya" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue1FaceDrMorichika" Value="We have our winners!,Alice and Shanghai!" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue2FaceMari" Value="You did it，Alice!" Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue3FaceRika" Value="That was such an intense,battle! It was!" Face="Rika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue4FaceShanghai" Value="That’s what you get，,you cheeky midget!,Now get out of my sight!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue5FaceRemilia" Value="........." Face="Remilia" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue6FaceShanghai" Value="....Huh?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue7FaceRemilia" Value="W...." Face="Remilia" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage7Dialogue8FaceRemilia" Value="WAAAAAAAAAAAAAH!!" Face="Remilia" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage9Dialogue1FaceMarisa" Value="Sheesh，ya made her cry." Face="Marisa" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage9Dialogue2FaceShanghai" Value="....Maybe I went too far." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage9Dialogue3FaceMari" Value="We can’t just let her run off on,her own. Let’s go find her." Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage9Dialogue4FaceRika" Value="Yeah!" Face="Rika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage11Dialogue1FaceSakuya" Value="Pardon me...." Face="Sakuya" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage11Dialogue2FaceShanghai" Value="Huh? She left you here?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage11Dialogue3FaceSakuya" Value="Milady....,She always gets like this,when she loses." Face="Sakuya" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage11Dialogue4FaceSakuya" Value="Would you say she hates,losing? How do I put it...." Face="Sakuya" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage11Dialogue5FaceShanghai" Value="....I get the idea.,Anyways，we’ve got to find,Remilia." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Remilia2Stage11Dialogue6FaceSakuya" Value="Much appreciated...." Face="Sakuya" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Rinnosuke3Stage1Dialogue1FaceDrMorichika" Value="There are only a few places,where unauthorized visitors,can go，so we should be" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Rinnosuke3Stage1Dialogue2FaceDrMorichika" Value="able to find her soon，but....,You really made a mess，huh?" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage1Dialogue1FaceMari" Value="Cough....,Here too!?" Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage1Dialogue2FaceMari" Value="Dad!" Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage3Dialogue1FaceMari" Value="....He passed out，but he’s,still alive!" Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage3Dialogue2FaceShanghai" Value="Get him out of here，quickly!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage5Dialogue1FaceAlice" Value="Wha!?,The door won’t open!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage5Dialogue2FaceShanghai" Value="Fire shutter locked due to,emergency!?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage5Dialogue3FaceShanghai" Value="You’re kidding me!,What about the victims,trapped inside!?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage5Dialogue4FaceMarisa" Value="I’ll try to unlock it from,the computer!" Face="Marisa" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage5Dialogue5FaceAlice" Value="Cough....,Then I’ll...." Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari5Stage5Dialogue6FaceShanghai" Value="Jack into the purifier!,It’ll be quicker to tackle the,cause!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Mari6Stage1Dialogue1FaceMari" Value="Cough....,Gotta unlock the door....!" Face="Mari" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter2Rinnosuke4Stage1Dialogue1FaceNone" Value="Dr. Morichika collapsed.,It looks like he passed out." Face="None" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue1FaceYamame" Value="Hm?,The professor has guests?" Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue2FaceAlice" Value="Dr. Morichika isn’t here?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue3FaceShanghai" Value="It’s kinda bad form to ask us to,come and then not show up." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue4FaceYamame" Value="Ahaha!,They called for him over the,intercom a little while ago." Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue5FaceYamame" Value="He’ll be back in a bit，so you,can wait here if you want." Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue6FaceAlice" Value="And you are?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue7FaceYamame" Value="I’m Yamame Kurodani.,I’m a research student under,Dr. Morichika." Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue8FaceShanghai" Value="And since when did network,specialists wear lab coats?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue9FaceYamame" Value="Does it really look that weird?" Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue10FaceYamame" Value="I wasn’t originally in the Tech,Department. I used to be in,the Pharmacy Department." Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue11FaceAlice" Value="The more I hear，,the less I understand...." Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue12FaceYamame" Value="Seeing is believing!,Try sending this data to,your Navi." Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue13FaceAlice" Value="?,Um，okay...." Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue14FaceShanghai" Value="What’s this? A drink?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue15FaceShanghai" Value="Gulp!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue16FaceAlice" Value="Wait! It might be....!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue17FaceShanghai" Value="It’s....!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue18FaceShanghai" Value="DEEELIIICIOOOUUUS!!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue19FaceYamame" Value="I know，right?" Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue20FaceYamame" Value="The theme of my research is,”Making Biologically,Delicious Cyber Drinks!”" Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue21FaceAlice" Value="How’s that related to,pharmacy....?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue22FaceYamame" Value="Um....,So we can drink medicine,that tastes good....?" Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue23FaceShanghai" Value="Basically，don’t think about it!,Thanks for the drink!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue24FaceYamame" Value="That’s how university research,goes. Gotta do it to graduate...." Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue25FaceYamame" Value="I don’t have a Navi，but,it’d be nice to get one for,poison tasting." Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue26FaceShanghai" Value="Wait.... Poison tasting!?,So that drink could have,made me sick!?" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue27FaceAlice" Value="(What a strange person....)" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue28FaceDrMorichika" Value="Sorry to keep you waiting.,Oh，Alice is here?" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue29FaceYamame" Value="Professor!,I finished my experiment，,so may I go home?" Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue30FaceDrMorichika" Value="Oh，alright.,Thanks for your hard work." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage1Dialogue31FaceYamame" Value="Great!,Well，see you later!" Face="Yamame" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage3Dialogue1FaceAlice" Value="She’s a pretty loud person...." Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage3Dialogue2FaceShanghai" Value="Oh yeah! PRO-FES-SOR!,Tell us what’s inside our,compressed data!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage3Dialogue3FaceDrMorichika" Value="Now now，take it easy.,First，let me show you this." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue1FaceAlice" Value="Is this....the analyzed data?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue2FaceDrMorichika" Value="Correct. It has multiple layers,of protection and compression." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue3FaceDrMorichika" Value="Even so，I was able to analyze,it up until the final step." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue4FaceDrMorichika" Value="The last layer of protection is,pretty tough...." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue5FaceDrMorichika" Value="I think it’s meant to be installed,in a Navi，but...." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue6FaceDrMorichika" Value="It seems like we need to be,near some kind of key for it,to activate." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue7FaceAlice" Value="Some kind of key?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue8FaceDrMorichika" Value="That’s all I know...." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue9FaceShanghai" Value="If we’re one step away，,then we just have to take,that final step!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue10FaceShanghai" Value="Alice! Get those legs moving,and investigate!!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue11FaceAlice" Value="Sure thing!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue12FaceAlice" Value="I want to know what this data,is hiding too!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue13FaceDrMorichika" Value="Then you should install this,analyzed data into your Navi." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue14FaceAlice" Value="Is it really safe to install data,like that?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue15FaceDrMorichika" Value="It’s fine.,This data was originally made,to be installed into a Navi." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue16FaceShanghai" Value="Besides，Navis can clean,themselves to some degree." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue17FaceShanghai" Value="Even if it’s bad data，,installing it now won’t,cause any problems." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue18FaceAlice" Value="If Shanghai says so，,then I guess it’s okay....?" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue19FaceDrMorichika" Value="Alright then.,Please wait for just a moment." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue20FaceShanghai" Value="Ah.... Ahn....,Nooo...." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue21FaceDrMorichika" Value="....Please don’t make weird,noises. People will get the,wrong idea." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue22FaceShanghai" Value="It’s just a joke. As if I’d ever,react to you like that.,Just install the data already." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue23FaceDrMorichika" Value="....There! Done!" Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue24FaceShanghai" Value="I feel pretty much the same,so far." Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue25FaceDrMorichika" Value="If you get near the key，,the program should activate." Face="DrMorichika" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue26FaceAlice" Value="I see! Interesting!" Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue27FaceAlice" Value="Let’s go look for info about,that key." Face="Alice" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue28FaceShanghai" Value="If we want to gather info，,the Square is the place to go!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3YamameStage5Dialogue29FaceShanghai" Value="Let’s go investigate around,the Square!" Face="Shanghai" Mono="False" />
  <Dialogue Key="Map.GenUniversity2frinChapter3Rinnosuke2AndYamameStage1Dialogue1FaceDrMorichika" Value="Shanghai should become,stronger if that data is usable." Face="DrMorichika" Mono="False" />
