<data>
  <!-- インターネット　ゲンソウスクエア -->
  <Text Key="Map.GenNetSquareName" Value="Genso Square" />
  <!-- 私はサブチップ商人です -->
  <Dialogue Key="Map.GenNetSquareASub-chipMerchantStage1Dialogue1FaceNaviBlueRobot" Value="I’m a Sub Chip vendor." Face="NaviBlueRobot" Mono="False" />
  <!-- サブチップを買いますか？ -->
  <Dialogue Key="Map.GenNetSquareASub-chipMerchantStage1Dialogue2QuestionFaceNaviBlueRobot" Value="Care to buy some?" Face="NaviBlueRobot" Mono="False" />
  <!-- はいいいえ -->
  <Text Key="Map.GenNetSquareASub-chipMerchantStage1Dialogue2Options" Value="Yes,No" />
  <!-- 私はサブチップ商人ですサブチップを買いますか？ -->
  <Dialogue Key="Map.GenNetSquareSub-chipShopStage1Dialogue1QuestionFaceNaviBlueRobot" Value="I’m a Sub Chip vendor.,Care to buy some?" Face="NaviBlueRobot" Mono="False" />
  <!-- はいいいえ -->
  <Text Key="Map.GenNetSquareSub-chipShopStage1Dialogue1Options" Value="Shop,Leave" />
  <!-- 私はサブチップ商人ですサブチップを買いますか？ -->
  <Dialogue Key="Map.GenNetSquareSub-chipShopStage4Dialogue1QuestionFaceNaviBlueRobot" Value="I’m a Sub Chip vendor.,Care to buy some?" Face="NaviBlueRobot" Mono="False" />
  <!-- はいいいえ -->
  <Text Key="Map.GenNetSquareSub-chipShopStage4Dialogue1Options" Value="Shop,Leave" />
  <!-- その荷物データ････君が依頼を受けてくれた子だね -->
  <Dialogue Key="Map.GenNetSquareRequest1RecipientStage1Dialogue1FaceNaviOrangeMale" Value="That DataPack....,So you’re the kid who took,the request?" Face="NaviOrangeMale" Mono="False" />
  <!-- シャンハイは荷物データを渡した -->
  <Dialogue Key="Map.GenNetSquareRequest1RecipientStage1Dialogue2FaceNone" Value="Shanghai handed over the,DataPack." Face="None" Mono="False" />
  <!-- ありがとう！これでサブチップを安めに売ることができるよ！ -->
  <Dialogue Key="Map.GenNetSquareRequest1RecipientStage1Dialogue3FaceNaviOrangeMale" Value="Thank you!,With this，I can cut the price,of my Sub Chips!" Face="NaviOrangeMale" Mono="False" />
  <!-- 報酬だね？じゃあこれをあげよう -->
  <Dialogue Key="Map.GenNetSquareRequest1RecipientStage1Dialogue4FaceNaviOrangeMale" Value="A reward?,Okay，here you go." Face="NaviOrangeMale" Mono="False" />
  <!-- 感謝するよ　ありがとう！ -->
  <Dialogue Key="Map.GenNetSquareRequest1RecipientStage1Dialogue5FaceNaviOrangeMale" Value="Thanks again!" Face="NaviOrangeMale" Mono="False" />
  <!-- 君のおかげでサブチップが安く売れるようになったからよかったら買って行ってくれ -->
  <Dialogue Key="Map.GenNetSquareRequest1RecipientStage3Dialogue1FaceNaviOrangeMale" Value="Thanks to you，I can cut,the price of my Sub Chips!,Buy some if you want!" Face="NaviOrangeMale" Mono="False" />
  <!-- 私はネット商人ですいいものありますよ？ -->
  <Dialogue Key="Map.GenNetSquareChipShopStage1Dialogue1QuestionFaceNaviGreenRobot" Value="I’m a Net vendor.,See anything you like?" Face="NaviGreenRobot" Mono="False" />
  <!-- 買う買わない -->
  <Text Key="Map.GenNetSquareChipShopStage1Dialogue1Options" Value="Shop,Leave" />
  <!-- お買い物でしたらカウンターの向こうからお願いします -->
  <Dialogue Key="Map.GenNetSquareShopClerkStage1Dialogue1FaceNaviPinkFemale" Value="If you’re here to shop，please,talk to me from the other side,of the counter." Face="NaviPinkFemale" Mono="False" />
  <!-- お買い物でしたらカウンターの向こうからお願いします -->
  <Dialogue Key="Map.GenNetSquareButler’sOfficeReceptionStage1Dialogue1FaceNaviPinkFemale" Value="If you’re here to shop，please,talk to me from the other side,of the counter." Face="NaviPinkFemale" Mono="False" />
  <!-- バトラーズオフィスへようこそ！こちらではＳＰウィルス討伐の依頼をあっせんしています -->
  <Dialogue Key="Map.GenNetSquareButlersOfficeStage1Dialogue1FaceNaviPinkFemale" Value="Welcome to the,NetBattlers’ Office!,Here for virus missions?" Face="NaviPinkFemale" Mono="False" />
  <!-- 依頼掲示板だ -->
  <Dialogue Key="Map.GenNetSquareRequestBulletinBoardStage1Dialogue1FaceNone" Value="It’s a Request Board." Face="None" Mono="False" />
  <!-- ざつだん掲示板だ -->
  <Dialogue Key="Map.GenNetSquareBulletinBoardStage1Dialogue1FaceNone" Value="It’s a Chat Board." Face="None" Mono="False" />
  <!-- あの人達なんなんですか？なんだか怖いです -->
  <Dialogue Key="Map.GenNetSquareChapter1ManNavi1Stage1Dialogue1FaceNaviGreenRobot" Value="What’s up with those Navis?,They’re kinda scary." Face="NaviGreenRobot" Mono="False" />
  <!-- さすがシャンハイさん！おかげで平和になりました！ -->
  <Dialogue Key="Map.GenNetSquareChapter1ManNavi1Stage3Dialogue1FaceNaviGreenRobot" Value="Thanks for restoring peace,to the land，Shanghai!" Face="NaviGreenRobot" Mono="False" />
  <!-- ここで売ってるチップを買えばあいつらに勝てるかなあ････ -->
  <Dialogue Key="Map.GenNetSquareChapter1ManNavi1DupeStage1Dialogue1FaceNaviOrangeMale" Value="Maybe I can beat those guys,if I buy some of the Sub Chips,they’re selling here...." Face="NaviOrangeMale" Mono="False" />
  <!-- 僕が迷ってる間に解決したみたいだね････ -->
  <Dialogue Key="Map.GenNetSquareChapter1ManNavi1DupeStage3Dialogue1FaceNaviOrangeMale" Value="Looks like you solved the,problem before I could act...." Face="NaviOrangeMale" Mono="False" />
  <!-- お　俺は何もしらねえんだ！本当だよ！ -->
  <Dialogue Key="Map.GenNetSquareChapter1ManNavi2Stage1Dialogue1FaceNaviOrangeMale" Value="I-I don’t know anything!,Honest!" Face="NaviOrangeMale" Mono="False" />
  <!-- 助けてくれてありがとう！すごいな　君は！ -->
  <Dialogue Key="Map.GenNetSquareChapter1ManNavi2Stage3Dialogue1FaceNaviOrangeMale" Value="Thanks for saving me!,You’re amazing!" Face="NaviOrangeMale" Mono="False" />
  <!-- うわ〜ん！あのお兄ちゃんをだれか助けてあげて！ -->
  <Dialogue Key="Map.GenNetSquareChapterNavi1Stage1Dialogue1FaceNaviChild" Value="Uwaaah!,Someone please help that,mister over there!" Face="NaviChild" Mono="False" />
  <!-- お姉ちゃんすごい！私もそれくらい強くなれるかな？ -->
  <Dialogue Key="Map.GenNetSquareChapterNavi1Stage3Dialogue1FaceNaviChild" Value="Big sis is so strong! Can I,be that strong someday?" Face="NaviChild" Mono="False" />
  <!-- あの乱暴なナビ達エイエンエリアの方から来たらしいわよ -->
  <Dialogue Key="Map.GenNetSquareChapterNavi2Stage1Dialogue1FaceNaviPinkFemale" Value="Seems like those rude Navis,came from Eien Area." Face="NaviPinkFemale" Mono="False" />
  <!-- あなたがここらで有名なシャンハイさんだったのね -->
  <Dialogue Key="Map.GenNetSquareChapterNavi2Stage3Dialogue1FaceNaviPinkFemale" Value="Aren’t you Shanghai?,The Navi who’s so popular,around here?" Face="NaviPinkFemale" Mono="False" />
  <!-- どおりで悪漢に対してもひるまないわけね尊敬しちゃうわ -->
  <Dialogue Key="Map.GenNetSquareChapterNavi2Stage3Dialogue2FaceNaviPinkFemale" Value="No wonder they respect you!,You’re not afraid of the bad,guys!" Face="NaviPinkFemale" Mono="False" />
  <!-- アニキ〜なんか言ってるよこいつ〜 -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviYoungerBrotherStage1Dialogue1FaceNaviPurplePunk" Value="Hey，bro〜!,Looks like this guy can tell us,somethin’〜!" Face="NaviPurplePunk" Mono="False" />
  <!-- ちっ！どいつもこいつもしらばっくれやがって････ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue1FaceNaviPurplePunk" Value="Tch!,Every single one of ’em acts,like they don’t know a thing...." Face="NaviPurplePunk" Mono="False" />
  <!-- ん？何だテメエ　俺達に文句あんのか？ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue2FaceNaviPurplePunk" Value="Huh? What do you want?,Got some kinda beef with us?" Face="NaviPurplePunk" Mono="False" />
  <!-- なめてると俺たち二人でてめえをデリートしちまうぞゴルァ！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue3FaceNaviPurplePunk" Value="You makin’ fun of us?,We’ll delete you!,URAAAAAAAAH!" Face="NaviPurplePunk" Mono="False" />
  <!-- あんたねえ････ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue4FaceShanghai" Value="You...." Face="Shanghai" Mono="False" />
  <!-- 取るに足らない量産型チンピラ如きが -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue5FaceShanghai" Value="You think you can defeat me，,this area’s supreme ruler?" Face="Shanghai" Mono="False" />
  <!-- ここ一帯を支配する力を持ったあたしにたった２対１で勝てると思うだなんて････ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue6FaceShanghai" Value="For two run-of-the-mill retail,Navis like you to fight me,two on one...." Face="Shanghai" Mono="False" />
  <!-- ばっかじゃないの！？ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue7FaceShanghai" Value="Are you guys idiots!?" Face="Shanghai" Mono="False" />
  <!-- なっにぃー？どうやら痛い目を見ないとわからないようだな -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue8FaceNaviPurplePunk" Value="Whaaaaa!? Looks like you,won’t understand unless we,beat it into ya!" Face="NaviPurplePunk" Mono="False" />
  <!-- 話が早くて助かるわさあアリス　こんな奴らさっさと片付けるわよ！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue9FaceShanghai" Value="Seems like diplomacy is off,the table. C’mon，Alice!,Let’s thrash ’em!" Face="Shanghai" Mono="False" />
  <!-- もう　仕方ないわね -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue10FaceAlice" Value="Jeez，I guess we have no,choice." Face="Alice" Mono="False" />
  <!-- いくわよ！シャンハイ！バトルオペレーション！#w@20#セット！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue11FaceAlice" Value="Let’s go，Shanghai!,Battle Routine!#w@20# Set!" Face="Alice" Mono="False" />
  <!-- イン！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage1Dialogue12FaceShanghai" Value="Execute!" Face="Shanghai" Mono="False" />
  <Text Key="Map.GenNetSquareChapter1EvilNaviBrotherBattle1Enemy1" Value="PunkBro" />
  <Text Key="Map.GenNetSquareChapter1EvilNaviBrotherBattle1Enemy2" Value="LilPunk" />
  <!-- ふん！口ほどにもないわね -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage3Dialogue1FaceShanghai" Value="Hmph!,All bark and no bite." Face="Shanghai" Mono="False" />
  <!-- こんなに強いナビがこのエリアにいたとは････ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage3Dialogue2FaceNaviPurplePunk" Value="How could there be such a,strong Navi in this Area....?" Face="NaviPurplePunk" Mono="False" />
  <!-- アニキ〜これじゃあ『上と下が交わる底』が何かわからないままですぜ〜 -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage3Dialogue3FaceNaviPurplePunk" Value="Bro，what’s ”where above and,below meet” even supposed,to mean!?" Face="NaviPurplePunk" Mono="False" />
  <!-- わ！バカ！お宝のヒントを教えてどうする！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage3Dialogue4FaceNaviPurplePunk" Value="Wah! You moron!,What if she hears the clue,to the treasure!?" Face="NaviPurplePunk" Mono="False" />
  <!-- あ！しまった！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage3Dialogue5FaceNaviPurplePunk" Value="Ah! Dang it!" Face="NaviPurplePunk" Mono="False" />
  <!-- はっは〜んお宝のヒント　ねえ････ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage3Dialogue6FaceShanghai" Value="Hmmmm〜?,The clue to the treasure，,huh....?" Face="Shanghai" Mono="False" />
  <!-- クソッ！引き上げるぞ！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage3Dialogue7FaceNaviPurplePunk" Value="Dang!,Retreat!" Face="NaviPurplePunk" Mono="False" />
  <!-- ちぇっ！逃したわね････ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage5Dialogue1FaceShanghai" Value="Tch!,They got away...." Face="Shanghai" Mono="False" />
  <!-- まあまあ　とりあえずこれで一件落着ね -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage5Dialogue2FaceAlice" Value="Well，at least that issue is,taken care of." Face="Alice" Mono="False" />
  <!-- お宝のヒントがゲットできたから良しとしましょうか -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage5Dialogue3FaceShanghai" Value="We got a clue to the treasure，,so I’d say it went well." Face="Shanghai" Mono="False" />
  <!-- 情けは人の為ならず　かしら -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage5Dialogue4FaceAlice" Value="Actually，I was talking about,helping someone in need." Face="Alice" Mono="False" />
  <!-- でも『上と下が交わる底』って一体何を表してるのかしら -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage5Dialogue5FaceAlice" Value="Anyways，what is ”where,above and below meet”,supposed to mean?" Face="Alice" Mono="False" />
  <!-- ゲンソウエリアのどこかにあることはわかってるんだからそれらしい所調べて回りましょ！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage5Dialogue6FaceShanghai" Value="It should be in Genso Area，,so let’s find a spot matching,the clue!" Face="Shanghai" Mono="False" />
  <!-- それもそうね！ -->
  <Dialogue Key="Map.GenNetSquareChapter1EvilNaviBrotherStage5Dialogue7FaceAlice" Value="Sounds good!" Face="Alice" Mono="False" />
  <!-- 掲示板は後で一気に見るよりもちょくちょく訪れたほうがいいんだぜ -->
  <Dialogue Key="Map.GenNetSquareChapter2MaleNavi1Stage1Dialogue1FaceNaviGreenRobot" Value="You shouldn’t read everything,on the Chat Board in one go." Face="NaviGreenRobot" Mono="False" />
  <!-- なにせ　情報は鮮度が命！だからな -->
  <Dialogue Key="Map.GenNetSquareChapter2MaleNavi1Stage1Dialogue2FaceNaviGreenRobot" Value="Instead，visit the Chat Board,regularly!,It keeps the info fresh!" Face="NaviGreenRobot" Mono="False" />
  <!-- バトラーズオフィスはネットバトラーの方々の活動を支援するサービスです -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue1FaceNaviBlueRobot" Value="The NetBattler’s Office is a,service designed to help,Netbattlers." Face="NaviBlueRobot" Mono="False" />
  <!-- 説明がいりますか？ -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue2FaceNaviBlueRobot" Value="Would you like to know more?" Face="NaviBlueRobot" Mono="False" />
  <!--  -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue3QuestionFaceNaviBlueRobot" Value="" Face="NaviBlueRobot" Mono="False" />
  <!-- 依頼掲示板についてＳＰウィルス討伐についていらないよ -->
  <Text Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue3Options" Value="The Request Board,SP Virus Missions,No thanks" />
  <!-- 依頼掲示板には様々な問題を抱えた人たちが依頼を書き込んでいます -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue4FaceNaviBlueRobot" Value="People with various problems,write requests on the Request,Board." Face="NaviBlueRobot" Mono="False" />
  <!-- これらの依頼を受けてそれを解決すると依頼主から報酬がもらえます -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue5FaceNaviBlueRobot" Value="Clients offer a reward to,whoever resolves their,request." Face="NaviBlueRobot" Mono="False" />
  <!-- 注意点として依頼は１度に１つしか受けることが出来ません -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue6FaceNaviBlueRobot" Value="I should note that you can only,accept one request at a time." Face="NaviBlueRobot" Mono="False" />
  <!-- すぐに解決できそうにない依頼を受けてしまわないように注意してくださいね -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue7FaceNaviBlueRobot" Value="Be careful not to take any,requests you can’t resolve,promptly." Face="NaviBlueRobot" Mono="False" />
  <!-- ＳＰウィルス討伐依頼はそこのカウンターで受けることが出来ます -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue8FaceNaviBlueRobot" Value="SP Virus Missions can be,taken at that counter over,there." Face="NaviBlueRobot" Mono="False" />
  <!-- ＳＰウィルスと呼ばれる強力なウィルスを倒すと賞金がもらえるといった依頼です -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue9FaceNaviBlueRobot" Value="Each mission has a bounty for,eliminating a powerful SP virus." Face="NaviBlueRobot" Mono="False" />
  <!-- ＳＰウィルスは危険なので普段は一般のナビと出会わないように････ -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue10FaceNaviBlueRobot" Value="Because SP viruses are so,dangerous，generic Navis,avoid them." Face="NaviBlueRobot" Mono="False" />
  <!-- 特殊なプロテクトをかけて互いに認識できないような処理をしています -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue11FaceNaviBlueRobot" Value="We run protective protocols,that make us invisible to,SP viruses." Face="NaviBlueRobot" Mono="False" />
  <!-- しかし　ＳＰウィルスが成長するとそのプロテクトが効かなくなり大きな被害が出てしまいます -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue12FaceNaviBlueRobot" Value="However，SP viruses can grow,strong enough to overcome,our protection." Face="NaviBlueRobot" Mono="False" />
  <!-- それを防ぐためにもＳＰウィルスは定期的に倒す必要があります -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue13FaceNaviBlueRobot" Value="To stop SP viruses from,harming us all，we must cull,them regularly." Face="NaviBlueRobot" Mono="False" />
  <!-- 依頼を受けると　受けた人に対してプロテクトを限定解除し戦えるようになります -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue14FaceNaviBlueRobot" Value="Upon taking a mission，,protective protocols will be,disabled." Face="NaviBlueRobot" Mono="False" />
  <!-- ＳＰウィルスは並のナビよりも強力なのでデリートされないよう気をつけてください -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue15FaceNaviBlueRobot" Value="SP viruses are more powerful,than normal Navis，so be,careful." Face="NaviBlueRobot" Mono="False" />
  <!-- 討伐依頼は１度につき１つしか受けることが出来ません -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue16FaceNaviBlueRobot" Value="You can only have one active,mission at a time." Face="NaviBlueRobot" Mono="False" />
  <!-- 自分の技量にあった相手を選ぶのが賢いやり方ですね -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue17FaceNaviBlueRobot" Value="It’s a good idea to target an,enemy around your skill level." Face="NaviBlueRobot" Mono="False" />
  <!-- 討伐完了した後にカウンターに報告すれば賞金を受け取れます -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue18FaceNaviBlueRobot" Value="After you complete the mission，,just claim your bounty at the,counter." Face="NaviBlueRobot" Mono="False" />
  <!-- そうですか････ -->
  <Dialogue Key="Map.GenNetSquareOfficeExplanationNaviStage1Dialogue19FaceNaviBlueRobot" Value="Is that so....?" Face="NaviBlueRobot" Mono="False" />
  <!-- ＳＰウィルスぶっ倒して賞金ふんだくろうと思ってたんだがよ････ -->
  <Dialogue Key="Map.GenNetSquareChapter2YouthNavi1Stage1Dialogue1FaceNaviOrangeMale" Value="I thought I’d beat an SP virus,and claim the bounty...." Face="NaviOrangeMale" Mono="False" />
  <!-- 反対にヤツにデリートされそうになってここへ逃げてきたのさ -->
  <Dialogue Key="Map.GenNetSquareChapter2YouthNavi1Stage1Dialogue2FaceNaviOrangeMale" Value="But I almost got deleted,instead! I had to run all the,way back here." Face="NaviOrangeMale" Mono="False" />
  <!-- ＳＰウィルスぶっ倒して賞金ふんだくろうと思ってたんだがよ････ -->
  <Dialogue Key="Map.GenNetSquareChapter2YouthNavi2Stage1Dialogue1FaceNaviOrangeMale" Value="I thought I’d beat an SP virus,and claim the bounty...." Face="NaviOrangeMale" Mono="False" />
  <!-- 反対にヤツにデリートされそうになってここへ逃げてきたのさ -->
  <Dialogue Key="Map.GenNetSquareChapter2YouthNavi2Stage1Dialogue2FaceNaviOrangeMale" Value="But I almost got deleted,instead! I had to run all the,way back here." Face="NaviOrangeMale" Mono="False" />
  <!-- インテリアはホームページに自由に置くことができる置物なんだけどね -->
  <Dialogue Key="Map.GenNetSquareChapter2GirlNavi1Stage1Dialogue1FaceNaviPinkFemale" Value="Interiors are decorations you,can place in your homepage." Face="NaviPinkFemale" Mono="False" />
  <!-- とっても値段が高いうえ役に立つものは少ないのよ！ -->
  <Dialogue Key="Map.GenNetSquareChapter2GirlNavi1Stage1Dialogue2FaceNaviPinkFemale" Value="They’re very expensive，and,most of them aren’t very useful!" Face="NaviPinkFemale" Mono="False" />
  <!-- あくまでも余ったお金で楽しむのがいいわよ！ -->
  <Dialogue Key="Map.GenNetSquareChapter2GirlNavi1Stage1Dialogue3FaceNaviPinkFemale" Value="That said，there are actually,some useful ones!" Face="NaviPinkFemale" Mono="False" />
  <!-- でも　中にはとっても役に立つ物もあるんだって！ -->
  <Dialogue Key="Map.GenNetSquareChapter2GirlNavi1Stage1Dialogue4FaceNaviPinkFemale" Value="If you have lots of money lying,around，you might enjoy them!" Face="NaviPinkFemale" Mono="False" />
  <!-- 私は体験版のみに存在するおまけスプライトです -->
  <Dialogue Key="Map.GenNetSquareTestSprite1Stage1Dialogue1FaceSprite" Value="I am a bonus sprite that only,exists in the trial verson." Face="Sprite" Mono="False" />
  <!-- 上位ウィルスとの戦闘を体験できるおまけを遊ぶことが出来ます -->
  <Dialogue Key="Map.GenNetSquareTestSprite1Stage1Dialogue2FaceSprite" Value="You can try your hand at a,bonus fight with high-rank,viruses." Face="Sprite" Mono="False" />
  <!-- １０連戦になりますがあそびますか？ -->
  <Dialogue Key="Map.GenNetSquareTestSprite1Stage1Dialogue3QuestionFaceSprite" Value="Attempt these 10 bonus,fights?" Face="Sprite" Mono="False" />
  <!-- はいいいえ -->
  <Text Key="Map.GenNetSquareTestSprite1Stage1Dialogue3Options" Value="Yes,No" />
